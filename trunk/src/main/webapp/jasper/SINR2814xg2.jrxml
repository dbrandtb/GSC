	<queryString><![CDATA[Select   r.dsreporte   ,
             r.nmreporte ,  
             c.DSUNIECO                          "Aseguradora",
             b.DSRAMO                            "Producto",
             a.OTVALOR13                         "Parentesco",
             count(a.OTVALOR06)                  "Cant. Reclamaciones",
             count(a.OTVALOR01)                  "Cant. Siniestro",
             sum(to_number(a.OTVALOR37))         "Importe de Siniestros",
             trunc((sum(to_number(a.OTVALOR37))/        
            (select sum(to_number(b.OTVALOR37))
             from   vTVALOPLA1 b   
              where  b.cdplanti   = a.cdplanti     and
                          b.cdelemento = a.cdelemento   and
                          b.cdunieco   = a.cdunieco     and
                          b.cdramo     = a.cdramo       and
                          to_date(b.otvalor08,'DD-MM-RRRR') between to_date($P{OTVALOR04},'DD-MM-RRRR') and
                                                                                                     to_date($P{OTVALOR05},'DD-MM-RRRR'))),4) * 100  "Porcentaje"
 from    vTVALOPLA1  a,
            TRAMOS    b,
            TPLANREP  p,
            MREPORTE r,
            TUNIDECO c
where  a.cdunieco   = c.cdunieco   and
             r.cdreporte  = p.cdreporte  and
             a.cdunieco   = c.cdunieco   and
             b.cdramo     = a.cdramo     and
             a.cdplanti   = p.cdplanti            and
             p.cdunieco   = a.cdunieco and
             p.cdelemento = a.cdelemento and
             p.cdramo     = a.cdramo     and 
             p.cdreporte  = $P{wCDREPORTE}   and
             a.cdelemento = to_number($P{OTVALOR03}) and
             a.cdunieco   = to_number($P{OTVALOR01}) and
             a.cdramo     = to_number($P{OTVALOR02}) and
             to_date(a.otvalor08,'DD-MM-RRRR')  between to_date($P{OTVALOR04},'DD-MM-RRRR') and
                                                                                         to_date($P{OTVALOR05},'DD-MM-RRRR')       
group by
       r.dsreporte   ,
       r.nmreporte ,  
       c.DSUNIECO   ,
       b.DSRAMO     ,
       a.cdplanti   ,
       a.cdelemento ,
       a.cdunieco   ,
       a.cdramo     ,
       a.OTVALOR13]]></queryString>

