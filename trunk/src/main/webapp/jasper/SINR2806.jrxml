	<queryString><![CDATA[select r.dsreporte                                                                                                                             Dsreporte               ,
       r.nmreporte                                                                                                                               Nmreporte              ,   
       a.OTVALOR03                                                                                                                         Companía               ,  
       u.DSELEMEN                                                                                                                           Descripcion            ,   
to_char(to_date(a.OTVALOR11,'dd-mm-rrrr'),'Month-rrrr','NLS_DATE_LANGUAGE=SPANISH') Fecha_Siniestro     ,
       count(a.OTVALOR09)                                                                                                               Numero_Siniestro  ,
       sum(to_number(a.OTVALOR29))                                                                                             Importe_Siniestro   ,                 
       round((sum(to_number(a.OTVALOR29)) /          
      (select sum(to_number(c.OTVALOR29))     
       from   vTVALOPLA2 c                      
       where  c.cdplanti   = a.cdplanti     and
              c.cdelemento = a.cdelemento   and
              c.cdunieco   = a.cdunieco     and
              c.cdramo     = a.cdramo       and
              to_date(c.otvalor11,'DD-MM-RRRR') between to_date($P{OTVALOR04},'DD-MM-RRRR') and
                                                                                        to_date($P{OTVALOR05},'DD-MM-RRRR'))) * 100,2) Porcentaje_Importe,
       to_char(to_date(a.OTVALOR11,'dd-mm-rrrr'),'rrrrmm')  Anio_Mes         
from   vTVALOPLA2 a ,    
       TPLANREP p ,
       MREPORTE r ,
       MDESTRUC u 
where  u.cdelemento  = p.cdelemento  and
       r.cdreporte   = p.cdreporte  and
       a.cdplanti    = p.cdplanti   and
       p.cdunieco = a.cdunieco and
       p.cdelemento = a.cdelemento and
       a.cdramo      = p.cdramo     and
       p.cdreporte   = $P{wCDREPORTE}        and
       a.cdelemento  = $P{OTVALOR03}         and
       a.cdunieco    = $P{OTVALOR01}           and
       a.cdramo      = $P{OTVALOR02}           and
       to_date(a.otvalor11,'DD-MM-RRRR')  between to_date($P{OTVALOR04},'DD-MM-RRRR') and
                                                                                  to_date($P{OTVALOR05},'DD-MM-RRRR')
group by
       r.dsreporte  ,
       r.nmreporte  ,  
       a.cdplanti   ,
       a.cdelemento ,
       a.cdunieco   ,
       a.cdramo     ,
       a.OTVALOR03  ,
       to_char(to_date(a.OTVALOR11,'dd-mm-rrrr'),'Month-rrrr','NLS_DATE_LANGUAGE=SPANISH'),       
       to_char(to_date(a.OTVALOR11,'dd-mm-rrrr'),'rrrrmm'),              
       u.DSELEMEN   
order by
       to_char(to_date(a.OTVALOR11,'dd-mm-rrrr'),'rrrrmm')]]></queryString>

