	<queryString><![CDATA[select p.cdreporte,
       p.dsreporte,
       p.nmreporte, 
       a.cdramo   ,
       a.cdasegur ,
       r.dsunieco ,
       a.cdplan,
       c.dsplan,
   count(*)   Cantidad,   --- adaptado.
round((count(*) / 
      (select count(*) 
       from   mpolisit d,
              mplanes  e
       where  e.cdramo    = d.cdramo    and
              e.cdplan    = d.cdplan    and
              e.cdtipsit  = d.cdtipsit  and  
              d.nmsuplem  = (select max(f.nmsuplem)
                             from   mpolisit f
                             where  f.cdunieco = d.cdunieco and
                                    f.cdramo   = d.cdramo   and
                                    f.status   = d.status   and
                                    f.estado   = d.estado   and
                                    f.nmpoliza = d.nmpoliza and
                                    f.nmsituac = d.nmsituac)  and
              d.status   = 'V'  and
              d.estado   = 'M'  and
              d.cdramo   = a.cdramo  and
              d.cdasegur = a.cdasegur)) * 100,3)  TotalxPlan 
from   mpolisit a,
       mplanes  c,
       mreporte p,
       tunideco r 
where  r.cdunieco  = a.cdasegur and 
       ---
       p.cdreporte = 11  and
       ---         
       c.cdramo    = a.cdramo    and
       c.cdplan    = a.cdplan    and
       c.cdtipsit  = a.cdtipsit  and  
       a.nmsuplem  = (select max(b.nmsuplem)
                      from   mpolisit b
                      where  b.cdunieco = a.cdunieco and
                             b.cdramo   = a.cdramo   and
                             b.status   = a.status   and
                             b.estado   = a.estado   and
                             b.nmpoliza = a.nmpoliza and
                             b.nmsituac = a.nmsituac) and 
       a.status   = 'V'  and
       a.estado   = 'M'  and
       a.cdramo   = to_number($P{OTVALOR02})  and
       a.cdasegur = to_number($P{OTVALOR01})
group by
       p.cdreporte,
       p.dsreporte,
       p.nmreporte, 
       a.cdramo   ,
       a.cdasegur ,
       r.dsunieco ,              
       a.cdplan,
       c.dsplan
order by
       c.dsplan]]></queryString>

