	<queryString><![CDATA[select a.cdunieco     ,
       d.dsunieco     ,
       a.cdramo       ,
       e.dsramo       ,
       b.dsreporte    ,
       a.cdelemento   ,
       f.dselemen     ,
       to_char(to_date(a.OTVALOR11,'dd-mm-rrrr'),'Month-rrrr','NLS_DATE_LANGUAGE=SPANISH') "Mes Anio",
       to_char(to_date(a.OTVALOR11,'dd-mm-rrrr'),'RRRRMM') "Anio Numerico",
       a.OTVALOR21    ,
       count(*)       ,
       ---       
      (select  count(*)
       from    vtvalopla2 h
       where  h.OTVALOR21   = a.OTVALOR21   and --- Debe tener una Descripción para la Cobertura (CSISA) 
                   h.cdunieco          = a.cdunieco          and
                   h.cdramo            = a.cdramo             and
                   h.cdelemento     = a.cdelemento      and
                   to_date(h.otvalor11,'DD-MM-RRRR') between to_date($P{OTVALOR04},'DD-MM-RRRR') and
                                                                                             to_date($P{OTVALOR05},'DD-MM-RRRR'))  "Cantidad"
       ---
from   vtvalopla2    a,
       mreporte    b,
       tplanrep    c,
       tunideco    d,
       tramos      e ,
       mdestruc  f
where  f.cdelemento = c.cdelemento        and 
       a.cdplanti        = c.cdplanti                 and
       d.cdunieco          = a.cdunieco            and
       e.cdramo            = a.cdramo              and
       c.cdelemento     = a.cdelemento         and
       c.cdunieco         = a.cdunieco             and
       c.cdramo           = a.cdramo                and
       c.cdplanti           = a.cdplanti                and
       c.cdreporte        = b.cdreporte             and 
       b.cdreporte        = $P{wCDREPORTE}      and
       a.OTVALOR21  is not null                    and --- Debe tener una Descripción para la Cobertura (CSISA) 
       a.cdunieco          = $P{OTVALOR01}          and
       a.cdramo            = $P{OTVALOR02}          and
       a.cdelemento      = $P{OTVALOR03}         and
       to_date(a.otvalor11,'DD-MM-RRRR') between to_date($P{OTVALOR04},'DD-MM-RRRR') and
                                                                                 to_date($P{OTVALOR05},'DD-MM-RRRR')
group by
       a.cdunieco     ,
       d.dsunieco     ,
       a.cdramo       ,
       e.dsramo       ,
       b.dsreporte    , 
       a.cdelemento   ,
       f.dselemen     ,
       to_char(to_date(a.OTVALOR11,'dd-mm-rrrr'),'Month-rrrr','NLS_DATE_LANGUAGE=SPANISH'),
       to_char(to_date(a.OTVALOR11,'dd-mm-rrrr'),'RRRRMM'),       
       a.OTVALOR21
order by 
       to_char(to_date(a.OTVALOR11,'dd-mm-rrrr'),'RRRRMM')]]></queryString>

