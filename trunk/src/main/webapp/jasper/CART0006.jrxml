	<queryString><![CDATA[select p.nmreporte,
        p.dsreporte,                             
        a.cdperpag ,
        b.dsperpag ,
        sum(e.ptimport),
       (select  count(*)
        from    mpolizas h,
                mpolagen1 q2
        where   q2.cduniage = to_number($P{OTVALOR01})   and
                q2.cdramo   = to_number($P{OTVALOR02})  and
                q2.estado   = 'M'             and
                q2.nmsuplem = (select max(q3.nmsuplem)
                               from   mpolagen1 q3
                               where  q3.cdunieco = q2.cdunieco  and
                                      q3.cdramo   = q2.cdramo    and
                                      q3.estado   = q2.estado    and
                                      q3.nmpoliza = q2.nmpoliza) and 
                h.cdperpag = a.cdperpag                and
                h.estado   = 'M'                       and
                h.cdunieco = q2.cdunieco               and
                h.nmpoliza = q2.nmpoliza               and
                h.cdramo   = q2.cdramo                 and
                h.nmsuplem = (select max(i.nmsuplem)
                              from   mpolizas i
                              where  i.estado    = h.estado     and
                                     i.cdunieco  = h.cdunieco   and
                                     i.cdramo    = h.cdramo     and
                                     i.nmpoliza  = h.nmpoliza)) Cantidad
        from   mpolizas  a,
               tperpag   b,
               mrecibo   e,
               mreporte  p,
               mpolagen1  q
        where  ---
               q.cduniage = to_number(30)   and
               q.cdramo   = to_number(500)  and
               q.estado   = 'M'             and
               q.nmsuplem = (select max(q1.nmsuplem)
                             from   mpolagen1 q1
                             where  q1.cdunieco = q.cdunieco  and
                                    q1.cdramo   = q.cdramo    and
                                    q1.estado   = q.estado    and
                                    q1.nmpoliza = q.nmpoliza) and 
               ---
               p.cdreporte = $P{wCDREPORTE}   and  
               e.cdestado  not in (2,4,5) and
               e.estado    = a.estado     and          
               e.nmpoliza  = a.nmpoliza   and
               e.cdramo    = a.cdramo     and
               e.cdunieco  = a.cdunieco   and      
               ---
               a.cdperpag  = b.cdperpag   and
               a.estado    = 'M'          and
               a.nmpoliza  = q.nmpoliza   and
               a.cdunieco  = q.cdunieco   and
               a.cdramo    = q.cdramo     and
               a.nmsuplem  = (select max(c.nmsuplem)             
                              from   mpolizas c                
                              where  c.estado   = a.estado   and
                                     c.cdunieco = a.cdunieco and
                                     c.cdramo   = a.cdramo   and
                                     c.nmpoliza = a.nmpoliza)
group by
        p.nmreporte,
        p.dsreporte,                             
        a.cdperpag,
        b.dsperpag 
order by
        a.cdperpag]]></queryString>

