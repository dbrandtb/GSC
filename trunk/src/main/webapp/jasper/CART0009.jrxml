	<queryString><![CDATA[select p.nmreporte,
       p.dsreporte,                             
       a.cdramo          Producto         ,
       k.dsramo          DesProducto   ,
       a.cdtipsit        TipoSituacion  ,
       c.dstipsit        DescripcionSit ,
       sum(d.nmimport)   TotPrima         ,
      (select count(*)
       from   mpolisit j
       where  j.cdunieco = to_number($P{OTVALOR01})             and
              j.cdramo   = to_number($P{OTVALOR02})             and
              j.estado   = 'M'                              and
              j.status   = 'V'                              and 
              j.cdtipsit = a.cdtipsit                       and
              j.nmsuplem = (select max(b.nmsuplem)             
                            from   mpolisit b                   
                            where  b.cdtipsit = j.cdtipsit  and
                                   b.cdunieco = j.cdunieco  and
                                   b.nmpoliza = j.nmpoliza  and
                                   b.cdramo   = j.cdramo    and
                                   b.estado   = j.estado    and 
                                   b.status   = j.status    and 
                                   b.nmsituac = j.nmsituac)) Cantidad 
from   mpolisit a,
       ttiposit c,
       mpolirec d,
       tramos   k,
       mreporte p,
       mpolagen1 q
where  --- 
       q.cduniage = to_number($P{OTVALOR01})   and
       q.cdramo   = to_number($P{OTVALOR02})   and
       q.estado   = 'M'             and
       q.nmsuplem = (select max(q1.nmsuplem)
                     from   mpolagen1 q1
                     where  q1.cdunieco = q.cdunieco  and
                            q1.cdramo   = q.cdramo    and
                            q1.estado   = q.estado    and
                            q1.nmpoliza = q.nmpoliza) and 
       ---
       a.nmpoliza  = q.nmpoliza  and
       p.cdreporte = $P{wCDREPORTE}  and  
       a.cdramo    = k.cdramo    and
       c.cdtipsit  = a.cdtipsit  and 
       d.cdunieco  = a.cdunieco  and
       d.cdramo    = a.cdramo    and
       d.estado    = a.estado    and
       d.status    = a.status    and 
       d.nmpoliza  = a.nmpoliza  and
       d.nmsuplem  = (select max(h.nmsuplem)
                      from   mpolirec h
                      where  h.nmsituac = d.nmsituac  and
                             h.cdunieco = d.cdunieco  and
                             h.cdramo   = d.cdramo    and
                             h.estado   = d.estado    and
                             h.status   = d.status    and 
                             h.nmpoliza = d.nmpoliza) and
       a.cdunieco  = q.cdunieco                       and
       a.cdramo    = q.cdramo                         and
       a.estado    = 'M'                              and
       a.status    = 'V'                              and 
       a.nmsuplem  = (select max(b.nmsuplem)
                      from   mpolisit b
                      where  b.cdunieco = a.cdunieco   and
                             b.nmpoliza = a.nmpoliza   and
                             b.cdramo   = a.cdramo     and
                             b.estado   = a.estado     and 
                             b.status   = a.status     and 
                             b.nmsituac = a.nmsituac)
group by
       p.nmreporte ,
       p.dsreporte ,                             
       a.cdtipsit  ,
       c.dstipsit  ,
       a.cdramo    ,
       k.dsramo
order by 
       a.cdtipsit  ,  
       c.dstipsit  ,
       a.cdramo    ,
       k.dsramo]]></queryString>

