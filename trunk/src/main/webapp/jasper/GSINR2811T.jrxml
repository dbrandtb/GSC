	<queryString><![CDATA[Select    r.dsreporte   ,
              r.nmreporte ,  
              c.DSUNIECO "Aseguradora",
              b.DSRAMO   "Producto",
              a.OTVALOR32 "Tipo Solicitud",
              count(a.OTVALOR06) "No. Reclamaciones",
              count(a.OTVALOR01) "No. Siniestro",
              sum(to_number(a.OTVALOR37)) "Importe de Siniestros",
              trunc((sum(to_number(a.OTVALOR37))/        
          (select sum(to_number(b.OTVALOR37))
           from   TVALOPLA b   
           where  b.cdplanti   = a.cdplanti     and
                       b.cdelemento = a.cdelemento   and
                       b.cdunieco   = a.cdunieco     and
                       b.cdramo     = a.cdramo       and 
                       to_date(b.otvalor08,'DD-MM-RRRR','NLS_DATE_LANGUAGE=AMERICAN') between 
                       to_date($P{OTVALOR04},'DD-MM-RRRR','NLS_DATE_LANGUAGE=AMERICAN') and
                       to_date($P{OTVALOR05},'DD-MM-RRRR','NLS_DATE_LANGUAGE=AMERICAN'))),4) * 100 "% Importe"
from   TVALOPLA  a,
          TRAMOS    b,
          TUNIDECO  c,
          TPLANREP p,
          MREPORTE r   
where   r.cdreporte  = p.cdreporte  and
             a.cdunieco   = c.cdunieco  and
             b.cdramo     = a.cdramo    and
             a.cdplanti   = p.cdplanti     and
             p.cdunieco = a.cdunieco and
             p.cdelemento = a.cdelemento and
             p.cdramo    = a.cdramo   and
            
             a.cdelemento = $P{OTVALOR03} and
             a.cdunieco   = $P{OTVALOR01}    and
             a.cdramo     = $P{OTVALOR02}   and
             to_date(a.otvalor08,'DD-MM-RRRR','NLS_DATE_LANGUAGE=AMERICAN') between 
             to_date($P{OTVALOR04},'DD-MM-RRRR','NLS_DATE_LANGUAGE=AMERICAN') and
             to_date($P{OTVALOR05},'DD-MM-RRRR','NLS_DATE_LANGUAGE=AMERICAN')
group by
       r.dsreporte   ,
       r.nmreporte ,  
       a.cdplanti   ,
       a.cdelemento ,
       a.cdunieco   ,
       a.cdramo     ,
       c.DSUNIECO   ,
       b.DSRAMO     ,       
       a.OTVALOR32]]></queryString>

