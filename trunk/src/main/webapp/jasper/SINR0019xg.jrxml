	<queryString><![CDATA[select q1.dsreporte   ,
       q1.nmreporte   ,
       q1.cdplanti    ,
       q1.cdelemento  ,
       q1.cdunieco    ,
       q1.cdramo      ,
       q1.Aseguradora , 
       q1.Producto    ,
       q1.Causa       ,
       q1.Mes         ,
       q1.MesCaracter ,
       q1.Cantidad    ,
       round(q1.Cantidad /         
            (select decode(count(j.OTVALOR01),0,1,count(j.OTVALOR01)) 
             from   vTVALOPLA1  j
             where  j.cdplanti   = q1.cdplanti      and
                    j.cdelemento = q1.cdelemento    and
                    j.cdunieco   = q1.cdunieco      and
                    j.cdramo     = q1.cdramo        and
                    to_char(to_date(j.otvalor08,'DD-MM-RRRR'),'RRRRMM') = q1.MesCaracter)* 100,3) PorSiniestro
from (select  r.dsreporte        dsreporte  ,
              r.nmreporte        nmreporte  ,
              a.cdplanti         cdplanti   ,
              a.cdelemento       cdelemento ,
              a.cdunieco         cdunieco   ,
              a.cdramo           cdramo     ,
              c.DSUNIECO                                                     Aseguradora , 
              b.DSRAMO                                                       Producto    ,
              a.OTVALOR25                                                    Causa       ,
              to_char(to_date(a.otvalor08,'DD-MM-RRRR'),'MM-RRRR')           Mes         ,
              to_char(to_date(a.otvalor08,'DD-MM-RRRR'),'RRRRMM')            MesCaracter ,
              count(a.OTVALOR01)                                             Cantidad    
      from  vTVALOPLA1     a    ,
              TRAMOS       b    ,
              TUNIDECO     c    ,
              TPLANREP     p    ,
              MREPORTE     r   
      where   r.cdreporte  = p.cdreporte  and
              a.cdunieco   = c.cdunieco   and
              a.cdunieco   = c.cdunieco   and
              b.cdramo     = a.cdramo     and
              p.cdunieco   = a.cdunieco   and
              p.cdelemento = a.cdelemento and
              p.cdramo     = a.cdramo     and
              a.cdplanti   = p.cdplanti   and        
  	      p.cdreporte  = $P{wCDREPORTE}           and 
              a.cdelemento = $P{OTVALOR03}        and
              a.cdunieco   = $P{OTVALOR01}           and
              a.cdramo     = $P{OTVALOR02}          and
              to_date(a.otvalor08,'DD-MM-RRRR') between to_date($P{OTVALOR04},'DD-MM-RRRR') and
                                                                                        to_date($P{OTVALOR05},'DD-MM-RRRR')              
      group by
              r.dsreporte   ,
              r.nmreporte   ,
              c.DSUNIECO    ,
              b.DSRAMO      ,
              a.cdplanti    ,
              a.cdelemento  ,
              a.cdunieco    ,
              a.cdramo      ,
              a.OTVALOR25   ,
              to_char(to_date(a.otvalor08,'DD-MM-RRRR'),'MM-RRRR'),
              to_char(to_date(a.otvalor08,'DD-MM-RRRR'),'RRRRMM')
      order by      
              to_number(to_char(to_date(a.otvalor08,'DD-MM-RRRR'),'RRRRMM')) Asc) q1]]></queryString>

