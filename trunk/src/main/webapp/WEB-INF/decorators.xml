<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/decorators">

	<excludes>
		<pattern>/nodecorate/*</pattern>
		<pattern>/resources/*</pattern>
		<pattern>/styles/*</pattern>
		<pattern>/scripts/*</pattern>
		<pattern>/images/*</pattern>
		<pattern>/dojo/*</pattern>
		<pattern>/struts/*</pattern>
		<pattern>/jsp/portal/portal.jsp</pattern>
		<pattern>/jsp/menu/menu.jsp</pattern>
		<pattern>/jsp/alertas/*.jsp</pattern>
		<pattern>/load.action</pattern>
		<pattern>/administracionCasos/obtenerAtributosVariablesCasos*
		</pattern>
	</excludes>


	<decorator name="empty" page="empty.jsp">
		<pattern>/seguridad/*</pattern>
		<pattern>/jsp/portal/login.jsp</pattern>
		<pattern>/editarAsegurados.action</pattern>
		<pattern>/jsp/extjs4/editarAsegurados.jsp</pattern>
		<pattern>/editarCoberturas.action</pattern>
		<pattern>/jsp/extjs4/aseguradoCoberturas.jsp</pattern>
		<pattern>/pantallaDomicilio.action</pattern>
		<pattern>/jsp/extjs4/aseguradoDomicilio.jsp</pattern>
		<pattern>/pantallaExclusion.action</pattern>
		<pattern>/jsp/extjs4/aseguradoExclusion.jsp</pattern>
		<pattern>/documentos/ventanaDocumentosPoliza</pattern>
		<pattern>/documentos/ventanaDocumentosPolizaClon</pattern>
		<pattern>/jsp/extjs4/ventanaDocumentosPoliza.jsp</pattern>
	</decorator>
    
    
	<decorator name="clean" page="clean.jsp">
		<pattern>/seguridad/cotizarSaludVital.action</pattern>
		<pattern>/cotizacionVital.action</pattern>
		<pattern>/jsp/extjs4/asegurados.jsp</pattern>

		<pattern>/dinosaurio.action</pattern>
		<pattern>/jsp/extjs4/denegado.jsp</pattern>

		<pattern>/datosComplementarios.action</pattern>
		<pattern>/jsp/extjs4/complementarios.jsp</pattern>

		<pattern>/subirArchivoMostrarPanel.action</pattern>
		<pattern>/jsp/extjs4/testFileUpload.jsp</pattern>

		<pattern>/subirArchivo.action</pattern>
		<pattern>/jsp/extjs4/subirArchivoReceptor.jsp</pattern>

		<pattern>/subirArchivoMostrarBarra.action</pattern>
		<pattern>/jsp/extjs4/subirArchivoBarra.jsp</pattern>
		
		<pattern>/procesoemision/busquedaPoliza.action</pattern>
		<pattern>/jsp/consultas/*.jsp</pattern>
		
		<!-- mesa de control -->
		<pattern>/mesacontrol/principal.action</pattern>
        <pattern>/jsp/extjs4/mesacontrol/principal.jsp</pattern>
        <!-- fin de mesa de control -->
	</decorator>
	
	
	<decorator name="default" page="default.jsp">
        <pattern>/ArbolRolCliente.action</pattern>
        <pattern>/seleccionaRolCliente.action</pattern>
        <pattern>/jsp/portal/treeRolElemento.jsp</pattern>
        <pattern>/jsp/portal/index.jsp</pattern>
    </decorator>
    
    
    <decorator name="decmainbody" page="decmainbody.jsp">
        <pattern>/jsp/portal/mainBody.jsp</pattern>
    </decorator>


</decorators>