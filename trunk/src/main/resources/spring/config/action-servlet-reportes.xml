<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">


    <bean id="reportesPrincipalAction" class="mx.com.aon.portal.web.reportes.ReportesPrincipalAction" scope="prototype">
        <property name="reportesManager" ref="reportesManager"/>
        <property name="exportMediator" ref="exportMediator"/>
        <property name="exportModel" ref="exportModelCatalogosGeneralesReportes"/>
    </bean>

    <bean id="reportesAgregarAction" class="mx.com.aon.portal.web.reportes.ReportesAgregarAction" scope="prototype">
        <property name="reportesAgregarManager" ref="reportesAgregarManager"/>
        <property name="mensajesErrorManager" ref="mensajesErrorManager"/>
    </bean>

    <bean id="reportesGeneradorAction" class="mx.com.aon.portal.web.reportes.ReportesGeneradorAction" scope="prototype">
        <property name="reportesManager" ref="reportesManager"/>
    </bean>

    <bean id="reportesGraficosAction" class="mx.com.aon.portal.web.reportes.ReportesGraficosAction" scope="prototype">
        <property name="reportesManager" ref="reportesManager"/>
        <property name="exportMediator" ref="exportMediator"/>
        <property name="exportModel" ref="exportModelPlan"/>
    </bean>

    <bean id="reportesProductosAction" class="mx.com.aon.portal.web.reportes.ReportesProductosAction" scope="prototype">
        <property name="reportesManager" ref="reportesManager" />
        <property name="exportMediator" ref="exportMediator"/>
        <property name="exportModel" ref="exportModelPlan"/>
        <property name="mensajesErrorManager" ref="mensajesErrorManager"/>
    </bean>

    <bean id="reportesAtributosAction" class="mx.com.aon.portal.web.reportes.ReportesAtributosAction" scope="prototype">
        <property name="reportesManager" ref="reportesManager" />
        <property name="exportMediator" ref="exportMediator"/>
        <property name="exportModel" ref="exportModelPlan"/>
    </bean>

    <bean id="reportesPlantillasAction" class="mx.com.aon.portal.web.reportes.ReportesPlantillasAction" scope="prototype">
        <property name="reportesManager" ref="reportesManager" />
        <property name="exportMediator" ref="exportMediator"/>
        <property name="exportModel" ref="exportModelCatalogosGeneralesReportes"/>
        <property name="mensajesErrorManager" ref="mensajesErrorManager"/>
    </bean>

    <bean id="reportesPlantillasAtributosAction" class="mx.com.aon.portal.web.reportes.ReportesPlantillasAtributosAction" scope="prototype">
        <property name="reportesManager" ref="reportesManager" />
        <property name="exportMediator" ref="exportMediator"/>
        <property name="exportModel" ref="exportModelPlan"/>
        <property name="mensajesErrorManager" ref="mensajesErrorManager"/>
    </bean>

    <bean id="reportesPlantillasAsociarAction" class="mx.com.aon.portal.web.reportes.ReportesPlantillasAsociarAction" scope="prototype">
        <property name="reportesManager" ref="reportesManager"/>
        <property name="exportMediator" ref="exportMediator"/>
        <property name="exportModel" ref="exportModelPlan"/>
    </bean>
    <!--
        Dependencia agregada de servicio que exporta los datos a un objeto con el modelo
        para poder ser exportados
    -->
    <bean id="exportModelCatalogosGeneralesReportes" class="mx.com.aon.portal.service.impl.reportes.ExportReportesManagerImpl" scope="prototype">
    </bean>


</beans>