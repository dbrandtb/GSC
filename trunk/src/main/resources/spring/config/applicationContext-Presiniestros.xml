<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	
	<!-- Presiniestros -->
		
	<bean id="presiniestrosManager" class="mx.com.aon.flujos.presinietros.service.impl.PresiniestrosManagerImpl" parent="abstractManagerJdbcTemplateInvoke">
	        <property name="abstractDAO">
	            <ref bean="presiniestrosDAO"/>
	        </property>
	</bean> 
  
	<bean id="dctosPresiniestrosManager" class="mx.com.aon.flujos.presinietros.service.impl.DctosPresiniestrosManagerImpl" parent="abstractManagerJdbcTemplateInvoke">
	        <property name="abstractDAO">
	            <ref bean="dctosPresiniestrosDAO"/>
	        </property>
	</bean> 
</beans>