<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<!-- WS ice2sigs -->
	<bean id="ice2sigsWebServices" class="mx.com.gseguros.ws.client.impl.Ice2sigsWebServicesImpl">
	   <property name="kernelManager" ref="kernelManagerSustituto"/>
	   <property name="endpoint" value="${url.ws.ice2sigs}"></property>
	   <property name="urlImpresionRecibos" value="${url.imp.recibos}"></property>
	</bean>	
	
	<bean id="servicioGSServiceCallbackHandlerImpl" class="mx.com.gseguros.ws.client.ice2sigs.callback.ServicioGSServiceCallbackHandlerImpl" scope="prototype">
	   <property name="kernelManager" ref="kernelManagerSustituto"/>
	</bean>
	
	
</beans>