<?xml version="1.0" encoding="ISO-8859-1"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="reglanegocio-endpoint" abstract="true"
		parent="parent-endpoint">
		<property name="templateModule" value="reglaNegocio" />
		<property name="xslModule" value="reglaNegocio" />
	</bean>

	<!-- Insert de reglas de negocio -->
	<bean id="INSERTAR_VARIABLE_TEMPORAL" parent="reglanegocio-endpoint">
		<property name="templateName" value="insertarVariableTemporal.vm" />
		<property name="transformResult" value="false" />
		<property name="unmarshallResult" value="false" />
	</bean>
	<bean id="INSERTAR_VALIDACION" parent="reglanegocio-endpoint">
		<property name="templateName" value="insertarValidacion.vm" />
		<property name="transformResult" value="false" />
		<property name="unmarshallResult" value="false" />
	</bean>
	<bean id="INSERTAR_CONCEPTO_TARIFICACION" parent="reglanegocio-endpoint">
		<property name="templateName" value="insertarConceptoTarificacion.vm" />
		<property name="transformResult" value="false" />
		<property name="unmarshallResult" value="false" />
	</bean>
	<bean id="INSERTAR_AUTORIZACION" parent="reglanegocio-endpoint">
		<property name="templateName" value="insertarAutorizacion.vm" />
		<property name="transformResult" value="false" />
		<property name="unmarshallResult" value="false" />
	</bean>
	<bean id="INSERTAR_CONDICION" parent="reglanegocio-endpoint">
		<property name="templateName" value="insertarCondicion.vm" />
		<property name="transformResult" value="false" />
		<property name="unmarshallResult" value="false" />
	</bean>
	
	<!-- Consulta de reglas de negocio -->
	<bean id="OBTENER_VARIABLE_TEMPORAL" parent="reglanegocio-endpoint">
		<property name="templateName" value="obtenerVariablesTemporales.vm" />
		<property name="xslName" value="listaReglasNegocio.xsl" />
		<property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
	</bean>
	<bean id="OBTENER_VALIDACION" parent="reglanegocio-endpoint">
		<property name="templateName" value="obtenerValidaciones.vm" />
		<property name="xslName" value="listaValidaciones.xsl" />
		<property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
	</bean>
	<bean id="OBTENER_CONCEPTO_TARIFICACION" parent="reglanegocio-endpoint">
		<property name="templateName" value="obtenerConceptosTarificacion.vm" />
		<property name="xslName" value="listaConceptosTarificacion.xsl" />
		<property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
	</bean>
	<bean id="OBTENER_AUTORIZACION" parent="reglanegocio-endpoint">
		<property name="templateName" value="obtenerAutorizaciones.vm" />
		<property name="xslName" value="listaAutorizaciones.xsl" />
		<property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
	</bean>
	<bean id="OBTENER_CONDICION" parent="reglanegocio-endpoint">
		<property name="templateName" value="obtenerCondiciones.vm" />
		<property name="xslName" value="listaReglasNegocio.xsl" />
		<property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
	</bean>
	
	<!-- Variables temporales asociadas al producto -->
	
	<bean id="OBTIENE_VARIABLES_TEMPORALES_PRODUCTO" parent="reglanegocio-endpoint">
		<property name="templateName" value="obtenerVariablesTemporalesAsociadasProducto.vm" />
		<property name="xslName" value="listaReglasNegocio.xsl" />
		<property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
	</bean>
	
	<bean id="ASOCIAR_VARIABLES_TEMPORALES_PRODUCTO" parent="reglanegocio-endpoint">
		<property name="templateName" value="asociaVariablesTemporalesProducto.vm" />
		<property name="transformResult" value="false" />
		<property name="unmarshallResult" value="false" />
	</bean>
	
	<bean id="DESASOCIAR_VARIABLES_TEMPORALES_PRODUCTO" parent="reglanegocio-endpoint">
		<property name="templateName" value="desasociaVariablesTemporalesProducto.vm" />
		<property name="transformResult" value="false" />
		<property name="unmarshallResult" value="false" />
	</bean>
	
	<bean id="ELIMINA_VAR_TMP" parent="reglanegocio-endpoint">
		<property name="templateName" value="eliminaVariableTemporal.vm" />
		<property name="transformResult" value="false" />
		<property name="unmarshallResult" value="false" />
	</bean>
	
</beans>