<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="tipoObjeto-endpoint" abstract="true"
		parent="parent-endpoint">
		<property name="templateModule" value="tipoObjeto" />
		<property name="xslModule" value="tipoObjeto" />
	</bean>

<!-- Bean declarado para obtener catalogo rol existente en BD-->

	<bean id="CATALOGO_TIPO_OBJETO" parent="tipoObjeto-endpoint">
		<property name="templateName" value="obtieneCatalogoTipoObjeto.vm" />
		<property name="xslName" value="obtieneCatalogoTipoObjeto.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>

	<bean id="AGREGAR_TIPO_OBJETO_CATALOGO" parent="tipoObjeto-endpoint">
		<property name="templateName" value="agregaTipoObjetoCatalogo.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>

	<bean id="OBTIENE_DATOS_VARIABLES_OBJETO" parent="tipoObjeto-endpoint">
		<property name="templateName" value="obtieneDatosVariablesObjeto.vm" />
		<property name="xslName" value="obtieneDatosVariablesObjeto.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="AGREGAR_TIPO_OBJETO_INCISO" parent="tipoObjeto-endpoint">
		<property name="templateName" value="agregaTipoObjetoInciso.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>
	
	<bean id="BORRAR_TIPO_OBJETO_INCISO" parent="tipoObjeto-endpoint">
		<property name="templateName" value="borraTipoObjetoInciso.vm" />
		
        <property name="xslModule" value="catalogs" />
        <property name="xslName" value="obtieneMensajes.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.model.MensajesVO" />
	</bean>
	
	<bean id="AGREGAR_DATOS_VARIABLES_OBJETO" parent="tipoObjeto-endpoint">
		<property name="templateName" value="agregaDatosVariablesObjeto.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>
	
	<bean id="BORRA_TATRI_OBJETO" parent="tipoObjeto-endpoint">
		<property name="templateName" value="borraTatriObjeto.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>
	
	<bean id="VALIDA_HIJOS_ATRIB_VAR_OBJETO" parent="tipoObjeto-endpoint">
		<property name="templateModule" value="atributosVariables" />
		<property name="templateName" value="validaHijosAtribVarObjeto.vm" />
		<property name="xslModule" value="atributosVariables" />
		<property name="xslName" value="validaHijosAtributosVariables.xsl" />
		<property name="unmarshallClass" value="mx.com.aon.portal.model.MensajesVO" />
	</bean>
	
</beans>

