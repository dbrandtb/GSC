	<bean id="LISTA_TABLA" parent="expresiones-endpoint">
		<property name="templateName" value="listaTabla.vm" />
		<property name="xslName" value="listaComboVariableConNick.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
-->
	<bean id="LISTA_COLUMNA" parent="expresiones-endpoint">
		<property name="templateName" value="listaColumna.vm" />
		<property name="xslName" value="listaComboVariableConNick.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	<bean id="LISTA_CLAVE" parent="expresiones-endpoint">
		<property name="templateName" value="listaClave.vm" />
		<property name="xslName" value="lista5ClavesVariableLocal.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="INSERTAR_EXPRESION" parent="expresiones-endpoint">
		<property name="templateName" value="insertarExpresion.vm" />
		<property name="xslName" value="obtieneUnEntero.xsl" />
		<property name="unmarshallClass" value="java.lang.Integer" />
	</bean>
	
	<bean id="OBTIENE_SECUENCIA_EXPRESION" parent="expresiones-endpoint">
		<property name="templateName" value="obtieneSecuenciaExpresion.vm" />
		<property name="xslName" value="obtieneUnEntero.xsl" />
		<property name="unmarshallClass" value="java.lang.Integer" />
	</bean>
	
	<bean id="INSERTAR_LISTA_VARIABLES" parent="expresiones-endpoint">
		<property name="templateName" value="insertarListaVariables.vm" />
		<property name="xslName" value="obtieneResultadoExpresiones.xsl" />
		<property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
	</bean>
	
	<bean id="INSERTAR_LISTA_CLAVES" parent="expresiones-endpoint">
		<property name="templateName" value="insertarListaClaves.vm" />
		<property name="xslName" value="obtieneResultadoExpresiones.xsl" />
		<property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
	</bean>
	
	<bean id="ELIMINAR_VARIABLE_LOCAL_EXPRESION" parent="expresiones-endpoint">
		<property name="templateName" value="eliminarVariableLocalExpresion.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>
	
	<bean id="OBTIENE_EXPRESION" parent="expresiones-endpoint">
		<property name="templateName" value="obtieneExpresion.vm" />
		<property name="xslName" value="obtieneExpresion.xsl" />
        <property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="OBTIENE_VARIABLE_EXPRESION" parent="expresiones-endpoint">
		<property name="templateName" value="obtieneVariableExpresion.vm" />
		<property name="xslName" value="obtieneVariableExpresion.xsl" />
        <property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="LISTA_FUNCIONES" parent="expresiones-endpoint">
		<property name="templateName" value="listaFunciones.vm" />
		<property name="xslName" value="listaFunciones.xsl" />
        <property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="LISTA_VARIABLES_TEMPORALES" parent="expresiones-endpoint">
		<property name="templateName" value="obtenerVariablesTemporales.vm" />
		<property name="xslName" value="listaVariablesTemporales.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="CLAVES_VARIABLE_LOCAL" parent="expresiones-endpoint">
		<property name="templateName" value="listaClavesVariableLocal.vm" />
		<property name="xslName" value="listaClaveVariableLocal.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="VALIDA_EXPRESION" parent="expresiones-endpoint">
		<property name="templateName" value="validarFormula.vm" />
		<property name="xslName" value="obtieneResultadoExpresiones.xsl" />
		<property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
	</bean>
	
</beans>
