<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="sumaAsegurada-endpoint" abstract="true"
		parent="parent-endpoint">
		<property name="templateModule" value="sumaAsegurada" />
		<property name="xslModule" value="sumaAsegurada" />
	</bean>

<!-- Bean declarado para obtener catalogo rol existente en BD-->

	<bean id="CATALOGO_TIPO_SUMA_ASEGURADA" parent="sumaAsegurada-endpoint">
		<property name="templateName" value="obtieneCatalogoTipoSumaAsegurada.vm" />
		<property name="xslName" value="obtieneCatalogosSumaAsegurada.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="CATALOGO_SUMA_ASEGURADA" parent="sumaAsegurada-endpoint">
		<property name="templateName" value="obtieneCatalogoSumaAsegurada.vm" />
		<property name="xslName" value="obtieneCatalogosSumaAsegurada.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="CATALOGO_MONEDA_SUMA_ASEGURADA" parent="sumaAsegurada-endpoint">
		<property name="templateName" value="obtieneCatalogoMonedaSumaAsegurada.vm" />
		<property name="xslName" value="obtieneCatalogosSumaAsegurada.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="LISTA_SUMAS_ASEGURADAS" parent="sumaAsegurada-endpoint">
		<property name="templateName" value="obtieneListaSumasAseguradas.vm" />
		<property name="xslName" value="obtieneListaSumasAseguradas.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="AGREGAR_SUMA_ASEGURADA" parent="sumaAsegurada-endpoint">
		<property name="templateName" value="agregarSumaAsegurada.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>
	
	<bean id="ELIMINAR_SUMA_ASEGURADA_PRODUCTO" parent="sumaAsegurada-endpoint">
		<property name="templateName" value="eliminarSumaAseguradaProducto.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>
	
	<bean id="LISTA_SUMAS_ASEGURADAS_INCISO" parent="sumaAsegurada-endpoint">
		<property name="templateName" value="obtieneListaSumasAseguradasInciso.vm" />
		<property name="xslName" value="obtieneListaSumasAseguradasInciso.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>

	<bean id="AGREGAR_SUMA_ASEGURADA_INCISO" parent="sumaAsegurada-endpoint">
		<property name="templateName" value="agregarSumaAseguradaInciso.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>

	<bean id="ELIMINAR_SUMA_ASEGURADA_INCISO" parent="sumaAsegurada-endpoint">
		<property name="templateName" value="eliminarSumaAseguradaInciso.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>

</beans>