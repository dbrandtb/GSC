<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="conceptosCobertura-endpoint" abstract="true"
		parent="parent-endpoint">
		<property name="templateModule" value="conceptosCobertura" />
		<property name="xslModule" value="conceptosCobertura" />
	</bean>

<!-- Bean declarado para obtener catalogo rol existente en BD-->

	<bean id="OBTIENE_CONCEPTOS_COBERTURA" parent="conceptosCobertura-endpoint">
		<property name="templateName" value="obtieneConceptosPorCobertura.vm" />
		<property name="xslName" value="obtieneConceptosPorCobertura.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="OBTIENE_LISTA_PERIODOS" parent="conceptosCobertura-endpoint">
		<property name="templateName" value="obtieneListaPeriodos.vm" />
		<property name="xslName" value="listaLlaveValor.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="OBTIENE_LISTA_COBERTURA" parent="conceptosCobertura-endpoint">
		<property name="templateName" value="obtieneListaCoberturas.vm" />
		<property name="xslName" value="listaLlaveValor.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="OBTIENE_LISTA_CONCEPTOS" parent="conceptosCobertura-endpoint">
		<property name="templateName" value="obtieneListaConceptos.vm" />
		<property name="xslName" value="listaLlaveValor.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="OBTIENE_LISTA_COMPORTAMIENTOS" parent="conceptosCobertura-endpoint">
		<property name="templateName" value="obtieneListaComportamientos.vm" />
		<property name="xslName" value="listaLlaveValor.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="OBTIENE_LISTA_CONDICIONES" parent="conceptosCobertura-endpoint">
		<property name="templateName" value="obtieneListaCondiciones.vm" />
		<property name="xslName" value="listaLlaveValor.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>

	<bean id="AGREGAR_CONCEPTO_COBERTURA" parent="conceptosCobertura-endpoint">
		<property name="templateName" value="agregarConceptoCobertura.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>
	
	<bean id="ELIMINAR_CONCEPTO_COBERTURA" parent="conceptosCobertura-endpoint">
		<property name="templateName" value="eliminarConceptoCobertura.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>
	
</beans>

