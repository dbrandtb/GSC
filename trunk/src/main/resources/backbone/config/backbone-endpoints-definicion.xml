<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
    
    <bean id="definicion-endpoint" abstract="true" parent="parent-endpoint" >
        <property name="templateModule" value="definicion" />
        <property name="xslModule" value="definicion" />
    </bean>

	<!-- 
	Beans agregado para el servicio de consulta de informacion de producto
	-->
    <bean id="EDITAR_PRODUCTO" parent="definicion-endpoint" >
        <property name="templateName" value="editarProducto.vm" />
        <property name="xslName" value="producto.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.catweb.configuracion.producto.definicion.model.ProductoVO" />
    </bean>
    
    <bean id="EDITAR_PRODUCTO_WIZARD" parent="definicion-endpoint" >
        <property name="templateName" value="editarProducto.vm" />
        <property name="xslName" value="producto_wizard.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.catweb.configuracion.producto.definicion.model.ProductoVO" />
    </bean>
    
    <bean id="INSERTAR_PRODUCTO" parent="definicion-endpoint" >
        <property name="templateName" value="insertarProducto.vm" />
        <property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
    </bean>
    
    <bean id="INSERTAR_TIPO_POLIZA" parent="definicion-endpoint" >
