<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
<include file="struts-default.xml" />

	<package name="checklistConfiguraCuenta" extends="json-default" namespace="/checklistConfiguraCuenta">		
		
		<action name="buscarConfiguraCuenta" class="listaChecklistCuentaAction" method="cmdBuscarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
		</action>
		<action name="irAgregarConfiguraCuenta" class="checklistConfiguraCuentaAction" method="cmdIrAgregarConfiguraCuentaClick">
	    	<result type="chain" name="agregarConfiguraCuenta">checkListCuenta</result>
	    </action>
	    <action name="irEditarConfiguraCuenta" class="checklistConfiguraCuentaAction" method="cmdIrEditarConfiguraCuentaClick">
	    	<result type="chain" name="editarConfiguraCuenta">checkListCuenta</result>
	    </action>
	    <action name="copiarConfiguraCuenta" class="copiaConfiguraCuentaAction" method="cmdCopiarConfiguraCuentaClick">
	    	<result type="json"><param name="ignoreHierarchy">false</param></result>
	    </action>
	    <action name="borrarConfiguraCuenta" class="checklistConfiguraCuentaAction" method="cmdBorrarClick">
	    	<result type="json"><param name="ignoreHierarchy">false</param></result>
	    </action>
	    
	</package>

</struts>