<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>


	<include file="struts-default.xml" />

	<package name="configuracionAtributosFormatoOrdenTrabajo" extends="json-default" namespace="/configuracionAtributosFormatoOrdenTrabajo">


    <action name="obtenerAtributosFormatoOrdenTrabajo" class="configuracionAtributosFormatosOrdenTrabajoAction" method="obtenerAtributosFormatoOrdenTrabajo">
        <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>

    <!-- usado para insert y update -->
    <action name="insertarGuardarAtributosFormatoOrdenTrabajo" class="configuracionAtributosFormatosOrdenTrabajoAction"  method="insertarGuardarAtributosFormatoOrdenTrabajo">
        <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
    
    <action name="borrarAtributosFormatoOrdenTrabajo" class="configuracionAtributosFormatosOrdenTrabajoAction"  method="borrarAtributosFormatoOrdenTrabajo">
        <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
    
   	
	</package>

</struts>
