<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>


	<include file="struts-default.xml" />

	<package name="numeroIncisos" extends="json-default" namespace="/numeroIncisos">


    <action name="buscarNumeroIncisos" class="listaNumeroIncisosAction" method="cmdBuscarClick">
        <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>

    
    <action name="insertarNumeroIncisos" class="numeroIncisosAction"  method="cmdAgregarClick">
        <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
    
    <action name="guardarNumeroIncisos" class="numeroIncisosAction"  method="cmdGuardarClick">
        <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
    
    <action name="borrarNumeroIncisos" class="numeroIncisosAction"  method="cmdBorrarClick">
        <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
    
    <action name="exportarNumeroIncisos" class="listaNumeroIncisosAction" method="cmdExportarClick">
				<result name="success" type="stream">
				<param name="contentType">${contentType}</param>
				<param name="inputName">inputStream</param>
				<param name="contentDisposition">filename="${filename}"</param>
				<param name="bufferSize">4096</param>
			</result>
	</action>       
	
 
    <action name="getNumeroInciso" class="numeroIncisosAction"  method="cmdGetClick">
        <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>

    <action name="loadTramos" class="numeroIncisosAction"  method="loadTramos">
        <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
    
    
	</package>

</struts>
