<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

    <include file="struts-default.xml" />                           

    <package name="cotizacionesMasivas" extends="json-default" namespace="/cotizacionesMasivas">
    
    
		
		<action name="buscarCotizacionMasiva" class="cotizacionMasivaAction" method="cmdBuscarCotizacionMasiva">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
		</action>
		
	
	<action name="aprobarCotizacion" class="cotizacionMasivaAction" method="cmdAprobarCotizacion">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
		</action>
		
	<action name="borrarCotizacion" class="cotizacionMasivaAction" method="cmdBorrarCotizacion">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
		</action>
		
    <action name="buscarTipoRamo" class="cotizacionMasivaAction" method="cmdBuscarTipoRamo">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
		</action>
		
	<action name="cargarArchivo" class="cotizacionMasivaAction" method="cmdCargarArchivo">
			<result name="success">/resources/jsp-script/configurador/pantallas/jsonResponseFileTree.jsp</result>
       </action>
    
    </package>

</struts>