<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

    <include file="struts-default.xml" />                           

    <package name="requisitosRehabilitacion" extends="json-default" namespace="/requisitosRehabilitacion">
    
    	<action name="getRequisitosRehabilitacion" class="requisitosRehabilitacionAction" method="cmdGetClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
        </action>
		
		<action name="buscarRequisitosRehabilitacion" class="listaRequisitosRehabilitacionAction" method="cmdBuscarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
		</action>
		
		
		 <action name="exportarRequisitosRehabilitacion" class="listaRequisitosRehabilitacionAction" method="cmdExportarClick">
			<result name="success" type="stream">
				<param name="contentType">${contentType}</param>
				<param name="inputName">inputStream</param>
				<param name="contentDisposition">filename="${filename}"</param>
				<param name="bufferSize">4096</param>
			</result>
		</action>        
			
	   <action name="insertarGuardarRequisitosRehabilitacion" class="requisitosRehabilitacionAction" method="cmdAgregarGuardarClick">
           <result type="json"><param name="ignoreHierarchy">false</param></result>
       </action>

      

        <action name="borrarRequisitosRehabilitacion" class="requisitosRehabilitacionAction" method="cmdBorrarClick" >
            <result type="json"><param name="ignoreHierarchy">false</param></result>
        </action>

        
     
    </package>

</struts>