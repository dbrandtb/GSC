<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>


	<include file="struts-default.xml" />

	<package name="consultarCasosSolicitud" extends="json-default" namespace="/consultarCasosSolicitud">


    <action name="buscarCasosSolicitud" class="listaConsultaCasosSolicitudAction" method="btnBuscarCasosClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
	</action>

    <action name="borrarCasosSolicitud" class="consultarCasosAction" method="cmdBorrarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
	</action>
	
	<action name="irConsultaCliente" class="consultarCasosAction" method="cmdIrConsultaClienteClick">
            <result type="chain" name="irPantallaConsultaCliente">consultaDeCliente</result>
	</action>
	
	<action name="irAltaCaso" class="consultarCasosAction" method="cmdIrAltaCasoClick">
            <result type="chain" name="irPantallaAltaCaso">altaCaso</result>
	</action>
	
	<action name="irConsultaCaso" class="consultarCasosAction" method="cmdIrConsultarCasosClick">
            <result type="chain" name="irPantallaConsultaCaso">consultarCasoSolicitud</result>
	</action>
	
	<!--action name="irConsultaCasoDetalle" class="consultarCasosAction" method="cmdIrConsultaCasoDetalleClick">
            <result type="chain" name="irPantallaConsultarCasoDetalle">consultarCasoDetalle</result>
	</action-->
	
	<action name="irConsultaCasoDetalle" class="listaAdministacionCasosAction" method="cmbIrConsultarCasoDetalleClick">
    	<exception-mapping result="irConsultarCasoDetalle" exception="javax.servlet.ServletException">consultarCasoDetalle</exception-mapping>
    	<exception-mapping result="irConsultarCasoDetalle" exception="java.sql.SQLException">consultarCasoDetalle</exception-mapping>    	
    	<exception-mapping result="irConsultarCasoDetalle" exception="java.lang.Exception">consultarCasoDetalle</exception-mapping>
        <result type="chain" name="irConsultarCasoDetalle">consultarCasoDetalle</result>
    </action>
