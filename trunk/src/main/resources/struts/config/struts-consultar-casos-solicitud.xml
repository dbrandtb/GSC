   <action name="exportarCasosSolicitud" class="listaConsultaCasosSolicitudAction" method="cmdExportarListadoCasosClick2">
        <result name="success" type="stream">
            <param name="contentType">${contentType}</param>
            <param name="inputName">inputStream</param>
            <param name="contentDisposition">filename="${filename}"</param>
            <param name="bufferSize">4096</param>
        </result>
    </action> 

    
    <action name="irReasignarCaso" class="consultarCasosAction" method="cmdIrReasignarCasoClick">
            <result type="chain" name="irReasignarCaso">reasignacionDeCaso</result>
	</action>

	</package>

</struts>
