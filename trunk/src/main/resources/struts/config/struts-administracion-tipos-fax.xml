<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>


	<include file="struts-default.xml" />

	<package name="administracionTiposFax" extends="json-default" namespace="/administracionTiposFax">


	<action name="buscarAtributosFax" class="listaAdministracionTiposFaxAction" method="cmdBuscarClick">
          <result type="json"><param name="ignoreHierarchy">false</param></result>
	</action> 
     
     <action name="getAtributosFaxReg" class="administracionTiposFaxAction" method="cmdGetClick">
          <result type="json"><param name="ignoreHierarchy">false</param></result>
	</action>
	
	 <action name="borrarAtributosFax" class="administracionTiposFaxAction" method="cmdBorrarClick">
          <result type="json"><param name="ignoreHierarchy">false</param></result>
	</action>
	
	<action name="guardarAtributosFax" class="administracionTiposFaxAction" method="cmdGuardarClick">
          <result type="json"><param name="ignoreHierarchy">false</param></result>
	</action>
	
	<action name="editarGuardarAtributosFax" class="administracionTiposFaxAction" method="cmdEditarGuardarClick">
          <result type="json"><param name="ignoreHierarchy">false</param></result>
	</action>
	
	<action name="exportarAtributosFax" class="listaAdministracionTiposFaxAction" method="cmdExportarClick">
				<result name="success" type="stream">
				   <param name="contentType">${contentType}</param>
		    <!-- <param name="contentType">application/binary</param> -->				   
				<param name="inputName">inputStream</param>
				<param name="contentDisposition">filename="${filename}"</param>
				<param name="bufferSize">4096</param>
			</result>
	</action>   
	<action name="actualizarAtributosFax" class="administracionTiposFaxAction" method="cmdActualizarClick">
          <result type="json"><param name="ignoreHierarchy">false</param></result>
	</action>

	</package>

</struts>
