<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>


	<include file="struts-default.xml" />

	<package name="actividadUsuario" extends="json-default" namespace="/actividadUsuario">


    <action name="buscarActividadUsuario" class="listaActividadUsuarioAction" method="cmdBuscarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
	</action>
    
    <action name="irActividadUsuarioResultado" class="listaActividadUsuarioAction" method="irActividadUsuarioResultadoClick">
             <result type="chain" name="actividadUsuarioResultado">actividadUsuarioResultado</result>
    </action>
    
     <action name="irActividadUsuario" class="listaActividadUsuarioAction" method="irActividadUsuarioClick">
             <result type="chain" name="actividadUsuario">actividadUsuario</result>
    </action>
    
    <action name="exportarActividadUsuario" class="listaActividadUsuarioAction" method="cmdExportarClick">
        <result name="success" type="stream">
             <param name="contentType">${contentType}</param>
            <param name="inputName">inputStream</param>
            <param name="contentDisposition">filename="${filename}"</param>
            <param name="bufferSize">4096</param>
        </result>
    </action>  
    
	</package>

</struts>
