<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

    <include file="struts-default.xml" />                           

    <package name="rehabilitacionManual" extends="json-default" namespace="/rehabilitacionManual">
		
		<action name="obtenerInformacionPoliza" class="rehabilitacionManualAction" method="cmdObtenerPoliza">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
		</action>
		
		<action name="obtenerRequisitos" class="rehabilitacionManualAction" method="cmdObtenerRequisitos">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
		</action>

		<action name="obtenerDatosCancelacion" class="rehabilitacionManualAction" method="cmdObtenerDatosCancelacion">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
		</action>

		<action name="rehabilitarPoliza" class="rehabilitacionManualAction" method="cmdRehabilitarPoliza">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
		</action>

		<action name="validarRecibosPagados" class="rehabilitacionManualAction" method="cmdValidarRecibosPagados">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
		</action>

		<action name="irRehabilitacionManual" class="rehabilitacionManualAction" method="irRehabilitacionManual">
            <result type="chain" name="irRehabilitacionManual">rehabilitacionManual</result>
		</action>
		
		<!-- <action name="irPolizasCanceladas" class="rehabilitacionManualAction" method="irPolizasCanceladas">
            <result type="chain" name="polizasCanceladas">polizasCanceladas</result>
		</action>  -->
		<action name="irConfiguracionRenovacion" class="rehabilitacionManualAction" method="irConfiguracionRenovacion">
            <result type="chain" name="consultaConfiguracionRenovacion">consultaConfiguracionRenovacion</result>
		</action>
    </package>

</struts>

