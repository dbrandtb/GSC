<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>


	<include file="struts-default.xml" />

	<package name="formatosDocumentos" extends="json-default" namespace="/formatosDocumentos">


    <action name="buscarAdministracionMantenimientoFax" class="listaAdministracionAtributosFaxAction" method="cmdBuscarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
	</action>
    
    <action name="borrarAdministracionMantenimientoFax" class="administracionMantenimientoFaxAction" method="cmdBorrarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>

    <action name="getAdministracionMantenimientoFax" class="administracionMantenimientoFaxAction" method="cmdGetClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>

    <action name="guardarAdministracionMantenimientoFax" class="administracionMantenimientoFaxAction" method="cmdGuardarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
    
    <action name="exportarAdministracionMantenimientoFax" class="listaAdministracionAtributosFaxAction" method="cmdExportarAdmAtrMntFaxClick">
        <result name="success" type="stream">
             <param name="contentType">${contentType}</param>
            <param name="inputName">inputStream</param>
            <param name="contentDisposition">filename="${filename}"</param>
            <param name="bufferSize">4096</param>
        </result>
    </action>  
    

	</package>

</struts>
