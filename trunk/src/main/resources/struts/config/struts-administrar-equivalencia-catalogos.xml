<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>


	<include file="struts-default.xml" />

	<package name="administrarEquivalenciaCatalogos" extends="json-default" namespace="/administrarEquivalenciaCatalogos">

	<!-- CONSULTA DE CASOS / SOLICITUDES  -->
    <action name="buscarEquivalenciaCatalogos" class="listaAdministrarEquivalenciaCatalogosAction" method="cmdBuscarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
	</action>
    
    <action name="exportarAtributosFax" class="listaAdministrarEquivalenciaCatalogosAction" method="cmdExportarClick">
            <result name="success" type="stream">
				<param name="contentType">${contentType}</param>
				<param name="inputName">inputStream</param>
				<param name="contentDisposition">filename="${filename}"</param>
				<param name="bufferSize">4096</param>
			</result>            
    </action>
    
    <action name="borrrarEquivalenciaCatalogo" class="administrarEquivalenciaCatalogosAction" method="cmdBorrarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
    
    <action name="agregarEquivalenciaCatalogo" class="administrarEquivalenciaCatalogosAction" method="cmdAgregarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
    
    <action name="getEquivalenciaCatalogoReg" class="administrarEquivalenciaCatalogosAction" method="cmdAgregarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
   
	<action name="irAdministrarCatalogSistemaExterno" class="administrarEquivalenciaCatalogosAction" method="cmdIrAdministrarCatalogSistemaExterno">
      <result type="chain" name="administrarCatalogSistemaExterno">administrarCatalogSistemaExterno</result>
    </action>
           
  
    
	</package>

</struts>
