	<queryString><![CDATA[select q.cduniage  ,
       a.cdunieco ,
       f.dsunieco        "Aseguradora",
       a.cdramo   , 
       g.dsramo          "Producto"   ,                  
       h.dsreporte       "Reporte"    , 
       a.cdgarant        "Cobertura"  ,
       i.dsgarant        "Descripcion", 
      (select count(*)
       from   mpoligar c      
       where  c.cdgarant = a.cdgarant    and 
              c.cdunieco = a.cdunieco    and 
              c.cdramo   = a.cdramo      and
              c.estado   = 'M'           and
              c.status   = 'V'           and
              c.nmsuplem = (select max(d.nmsuplem)
                            from   mpoligar d
                            where  d.cdgarant = c.cdgarant and
                                   d.nmsituac = c.nmsituac and
                                   d.status   = c.status   and
                                   d.estado   = c.estado   and
                                   d.cdunieco = c.cdunieco and
                                   d.cdramo   = c.cdramo   and
                                   d.nmpoliza = c.nmpoliza))    "Cantidad",
       sum(j.ptimport)   "Importe"  ,
       0                 "Dependientes"  
from   mpoligar a,
       tunideco f,
       tramos   g,
       mreporte h,
       tgaranti i,
       tcalreci j,
       mpolagen1 q         
where  --- 
       q.cduniage = to_number($P{OTVALOR01})  and
       q.cdramo   = to_number($P{OTVALOR02})  and
       q.estado   = 'M'                   and
       q.nmsuplem = (select max(q1.nmsuplem)
                     from   mpolagen1 q1
                     where  q1.cdunieco = q.cdunieco  and
                            q1.cdramo   = q.cdramo    and
                            q1.estado   = q.estado    and
                            q1.nmpoliza = q.nmpoliza) and 
       ---
       j.nmsuplem  = a.nmsuplem            and
       j.cdgarant  = a.cdgarant            and
       j.cdunieco  = a.cdunieco            and
       j.cdramo    = a.cdramo              and
       j.estado    = a.estado              and
       j.nmpoliza  = a.nmpoliza            and 
       i.cdgarant  = a.cdgarant            and
       h.cdreporte = 8                     and
       g.cdramo    = a.cdramo              and
       f.cdunieco  = q.cduniage            and
       a.cdunieco  = q.cdunieco            and 
       a.cdramo    = q.cdramo              and
       a.nmpoliza  = q.nmpoliza            and
       a.estado    = 'M'                   and
       a.status    = 'V'                   and
       a.nmsuplem  = (select max(b.nmsuplem)
                      from   mpoligar b
                      where  b.cdgarant = a.cdgarant and
                             b.nmsituac = a.nmsituac and
                             b.status   = a.status   and
                             b.estado   = a.estado   and
                             b.cdunieco = a.cdunieco and
                             b.cdramo   = a.cdramo   and
                             b.nmpoliza = a.nmpoliza)
group by
       q.cduniage ,
       a.cdunieco ,
       a.cdramo   ,
       a.cdgarant ,  
       i.dsgarant ,                    
       f.dsunieco ,
       g.dsramo   ,
       h.dsreporte
order by
       a.cdgarant]]></queryString>

