	<queryString><![CDATA[select  q.cduniage ,

          a.cdunieco ,

          a.cdramo   ,

          p.dsreporte,

          p.nmreporte,

          a.cdrol    ,

          b.dsrol    ,

       round((count(*) /

          (select  count(*)  Cantidad

           from    mpoliper   a1,

                   mpersona   c1,

                   mpolagen1   q1

          where    c1.otfisjur  = 'F'                   and --- Solo Personas Fisicas

                   a1.cdperson  = c1.cdperson            and

                   a1.status    = 'V'                   and --- Vigente/Vivo

                   ---                                                 

                   a1.nmpoliza  = q1.nmpoliza            and

                   a1.estado    = q1.estado              and --- Mestro

                   a1.cdramo    = q1.cdramo              and

                   a1.cdunieco  = q1.cdunieco            and

                   a1.nmsuplem  = (select max(b1.nmsuplem) 

                                  from   mpoliper b1       

                                  where  b1.nmsituac  = a1.nmsituac     and

                                         b1.status    = a1.status       and

                                         b1.estado    = a1.estado       and

                                         b1.cdunieco  = a1.cdunieco     and

                                         b1.cdramo    = a1.cdramo       and

                                         b1.nmpoliza  = a1.nmpoliza)    and

                   ---                                      

                   q1.nmsuplem  = (select max(q11.nmsuplem)

                                  from   mpolagen1 q11      

                                  where  q11.cdunieco = q1.cdunieco     and

                                         q11.cdramo   = q1.cdramo       and

                                         q11.estado   = q1.estado       and

                                         q11.nmpoliza = q1.nmpoliza)    and

                   q1.estado   = 'M'                     and

                   q1.cdramo   = a.cdramo          and

                   q1.cduniage = q.cduniage)) * 100, 2)   Cantidad

  from    mpoliper   a,

          trol       b,

          mpersona   c,

          mreporte   p,                  

          mpolagen1   q

 where   p.cdreporte = $P{wCDREPORTE}            and  

          c.otfisjur  = 'F'                   and --- Solo Personas Fisicas

          a.cdperson  = c.cdperson            and

          a.cdrol     = b.cdrol               and

          a.status    = 'V'                   and --- Vigente/Vivo

          ---                                                 

          a.nmpoliza  = q.nmpoliza            and

          a.estado    = q.estado              and --- Mestro

          a.cdramo    = q.cdramo              and

          a.cdunieco  = q.cdunieco            and

          a.nmsuplem  = (select max(b.nmsuplem)

                         from   mpoliper b

                         where  a.nmsituac  = b.nmsituac     and

                                b.status    = a.status       and

                                b.estado    = a.estado       and

                                b.cdunieco  = a.cdunieco     and

                                b.cdramo    = a.cdramo       and

                                b.nmpoliza  = a.nmpoliza)    and

          ---           

          q.nmsuplem  = (select max(q1.nmsuplem)

                         from   mpolagen1 q1

                         where  q1.cdunieco = q.cdunieco     and

                                q1.cdramo   = q.cdramo       and

                                q1.estado   = q.estado       and

                                q1.nmpoliza = q.nmpoliza)    and

          q.estado   = 'M'                    and

          
		   q.cdramo   = to_number($P{OTVALOR02})   and
          q.cduniage = to_number($P{OTVALOR01})   
		  

    group by

          q.cduniage ,

          a.cdunieco ,

          a.cdramo   ,

          p.dsreporte,

          p.nmreporte,

          a.cdrol,     

          b.dsrol]]></queryString>

