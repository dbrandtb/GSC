	<queryString><![CDATA[select p.nmreporte,
       p.dsreporte,                             
       b.cdforpag       "Código",
       d.dsforpag       "Descripción",
       sum(e.ptimport)  "Importe",
      (select count(*)
       from   mpolizas g,
              mpoliagr j,
              mpolagen1 q2
       where  ---
              q2.cduniage = to_number($P{OTVALOR01})   and
              q2.cdramo   = to_number($P{OTVALOR02})  and
              q2.estado   = 'M'             and
              q2.nmsuplem = (select max(q3.nmsuplem)
                             from   mpolagen1 q3
                             where  q3.cdunieco = q2.cdunieco  and
                                    q3.cdramo   = q2.cdramo    and
                                    q3.estado   = q2.estado    and
                                    q3.nmpoliza = q2.nmpoliza) and 
              ---
              j.cdforpag = b.cdforpag and          
              ---
              j.estado   = g.estado   and
              j.nmpoliza = g.nmpoliza and
              j.cdunieco = g.cdunieco and
              j.cdramo   = g.cdramo   and
              j.nmsuplem = (select max(k.nmsuplem)
                            from   mpoliagr k
                            where  k.estado   = j.estado    and
                                   k.cdunieco = j.cdunieco  and
                                   k.cdramo   = j.cdramo    and
                                   k.nmpoliza = j.nmpoliza) and
              ----
              g.estado   = 'M'                              and
              g.cdunieco = q2.cdunieco                      and
              g.nmpoliza = q2.nmpoliza                      and
              g.cdramo   = q2.cdramo                        and
              g.nmsuplem = (select max(h.nmsuplem)              
                            from   mpolizas h                     
                            where  h.estado   = g.estado    and
                                   h.cdunieco = g.cdunieco  and
                                   h.cdramo   = g.cdramo    and
                                   h.nmpoliza = g.nmpoliza)) "Cantidad de Pólizas"
   from   mpolizas a,
          mpoliagr b,
          tformpag d, 
          mrecibo  e,
          mreporte p,
          mpolagen1 q
   where  ---
          q.cduniage = to_number($P{OTVALOR01})   and
          q.cdramo   = to_number($P{OTVALOR02})  and
          q.estado   = 'M'             and
          q.nmsuplem = (select max(q1.nmsuplem)
                        from   mpolagen1 q1
                        where  q1.cdunieco = q.cdunieco  and
                               q1.cdramo   = q.cdramo    and
                               q1.estado   = q.estado    and
                               q1.nmpoliza = q.nmpoliza) and 
          ---
          p.cdreporte  = $P{wCDREPORTE} and  
          e.cdagrupa   = b.cdagrupa and
          e.estado     = b.estado   and          
          e.nmpoliza   = b.nmpoliza and
          e.cdramo     = b.cdramo   and
          e.cdunieco   = b.cdunieco and          
          ---
          b.cdforpag   = d.cdforpag and          
          ---
          b.estado     = a.estado   and
          b.nmpoliza   = a.nmpoliza and
          b.cdunieco   = a.cdunieco and
          b.cdramo     = a.cdramo   and
          b.nmsuplem   = (select max(f.nmsuplem)
                          from   mpoliagr f
                          where  f.estado   = a.estado    and
                                 f.cdunieco = a.cdunieco  and
                                 f.cdramo   = a.cdramo    and
                                 f.nmpoliza = a.nmpoliza) and
          ----
          a.estado     = 'M'        and
          a.cdunieco   = q.cdunieco and  
          a.nmpoliza   = q.nmpoliza and
          a.cdramo     = q.cdramo   and
          a.nmsuplem   = (select  max(c.nmsuplem)
                          from    mpolizas c
                          where   c.estado   = a.estado   and
                                  c.cdunieco = a.cdunieco and
                                  c.cdramo   = a.cdramo   and
                                  c.nmpoliza = a.nmpoliza)
   group by 
          p.nmreporte,
          p.dsreporte,                             
          b.cdforpag,
          d.dsforpag
   order by 
          b.cdforpag]]></queryString>

