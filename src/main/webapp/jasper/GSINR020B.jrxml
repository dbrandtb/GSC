	<queryString><![CDATA[Select q1.dsreporte        ,
       q1.nmreporte        ,
       q1.cdplanti         , 
       q1.cdelemento       ,
       q1.cdunieco         ,
       q1.cdramo           ,
       q1.Aseguradora      ,
       q1.Producto         ,
       q1.Causa            ,
       q1.Mes              ,
       q1.MesCaracter      ,
       q1.Importe_Siniestro,
       ----
  round(q1.Importe_Siniestro/     
       (select decode(sum(to_number(nvl(j.OTVALOR37,0))),0,1,sum(to_number(nvl(j.OTVALOR37,0)))) 
        from   TVALOPLA  j
        where  j.cdplanti   = q1.cdplanti      and
               j.cdelemento = $P{OTVALOR03}    and
               j.cdunieco   = $P{OTVALOR01}     and
               j.cdramo     = $P{OTVALOR02}        and
               to_char(to_date(j.otvalor08,'DD-MM-RRRR','NLS_DATE_LANGUAGE=AMERICAN'),'RRRRMM') = q1.MesCaracter) * 100,3) PorImporte        
       ----   
from  (select         
               r.dsreporte              dsreporte,
               r.nmreporte              nmreporte,
               a.cdplanti               cdplanti, 
               a.cdelemento             cdelemento,
               a.cdunieco               cdunieco,
               a.cdramo                 cdramo,
               c.DSUNIECO                                                     Aseguradora ,
               b.DSRAMO                                                       Producto    ,
               a.OTVALOR25                                                    Causa       ,
               to_char(to_date(a.otvalor08,'DD-MM-RRRR','NLS_DATE_LANGUAGE=AMERICAN'),'MM-RRRR')           Mes         ,
               to_char(to_date(a.otvalor08,'DD-MM-RRRR','NLS_DATE_LANGUAGE=AMERICAN'),'RRRRMM')            MesCaracter ,                              
               sum(to_number(nvl(a.OTVALOR37,0)))                                    Importe_Siniestro                
       from    TVALOPLA   a,
               TRAMOS      b,
               TUNIDECO   c,
               TPLANREP   p,
               MREPORTE  r  
       where   r.cdreporte  = p.cdreporte  and
               a.cdunieco   = c.cdunieco  and
               b.cdramo     = a.cdramo    and
               a.cdplanti   = p.cdplanti            and
               p.cdunieco   = a.cdunieco and
               p.cdelemento = a.cdelemento and
               p.cdramo     = a.cdramo   and
               p.cdreporte  = $P{wCDREPORTE}   and 
               a.cdelemento = $P{OTVALOR03}       and
               a.cdunieco   = $P{OTVALOR01}          and
               a.cdramo     = $P{OTVALOR02} 
           group by
               r.dsreporte   ,
               r.nmreporte   ,
               c.DSUNIECO   ,
               b.DSRAMO     ,
               a.cdplanti   ,
               a.cdelemento ,
               a.cdunieco   ,
               a.cdramo     ,
               a.OTVALOR25  ,
               to_char(to_date(a.otvalor08,'DD-MM-RRRR','NLS_DATE_LANGUAGE=AMERICAN'),'MM-RRRR'),
               to_char(to_date(a.otvalor08,'DD-MM-RRRR','NLS_DATE_LANGUAGE=AMERICAN'),'RRRRMM'),               
               to_number(to_char(to_date(a.otvalor08,'DD-MM-RRRR','NLS_DATE_LANGUAGE=AMERICAN'),'RRRRMM'))                        
       order by
               to_number(to_char(to_date(a.otvalor08,'DD-MM-RRRR','NLS_DATE_LANGUAGE=AMERICAN'),'RRRRMM')) Asc) q1]]></queryString>

