	<parameter name="OTVALOR01" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="OTVALOR02" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="CDUSUARIO" isForPrompting="false" class="java.lang.String"/>
	<parameter name="wCDREPORTE" isForPrompting="false" class="java.lang.Integer"/>
	<parameter name="OTVALOR06" isForPrompting="false" class="java.lang.String"/>
	<parameter name="OTVALOR07" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[select dsramo       ,
       dsunieco     ,  
       dsreporte    ,
       Rango_Importe,              
       count(Rango_Importe)     "Cantidad", 
       round(count(Rango_Importe) /      
      (select count(unique a.nmpoliza)
       from   mpolicap b,
              mpolizas a,
              mpolagen1 q
       where  ---
              q.cduniage = to_number($P{OTVALOR01})  and
              q.cdramo   = to_number($P{OTVALOR02})  and
              q.estado   = 'M'                   and
              q.nmsuplem = (select max(q1.nmsuplem)
                            from   mpolagen1 q1
                            where  q1.cdunieco = q.cdunieco  and
                                   q1.cdramo   = q.cdramo    and
                                   q1.estado   = q.estado    and
                                   q1.nmpoliza = q.nmpoliza) and 
              ---
              b.status   = 'V'          and
              b.estado   = a.estado     and
              b.nmsuplem = a.nmsuplem   and
              b.nmpoliza = a.nmpoliza   and
              b.cdunieco = a.cdunieco   and
              b.cdramo   = a.cdramo     and 
              a.cdunieco = q.cdunieco   and
              a.cdramo   = q.cdramo     and
              a.estado   = 'M'                             and
              a.nmsuplem = (select max(c.nmsuplem)      
                            from   mpolizas c
                            where  c.estado   = a.estado   and
                                   c.cdunieco = a.cdunieco and
                                   c.cdramo   = a.cdramo   and
                                   c.nmpoliza = a.nmpoliza)) * 100,3) "Porcentaje"
from (
      select i.dsramo      dsramo   ,
             h.dsunieco    dsunieco ,   
             g.dsreporte   dsreporte,
             case                                                                  
                when sum(b.ptcapita) < 184000                       then '(A) < 184000'
                when sum(b.ptcapita) between  184001   and  374400  then '(B) 184001  - 374400'     
                when sum(b.ptcapita) between  374001   and  565200  then '(C) 374001  - 565200'   
                when sum(b.ptcapita) between  565201   and  730000  then '(D) 565201  - 730000'    
                when sum(b.ptcapita) between  730001   and  936000  then '(E) 730001  - 936000'
                when sum(b.ptcapita) between  936001   and  1116000 then '(F) 936001  - 1116000'
                when sum(b.ptcapita) between  1116001  and  1332000 then '(G) 1116001 - 1332000'
                when sum(b.ptcapita) between  1332001  and 1548000  then '(H) 1332001 - 1548000'                              
                else '(I) > 1548001'
             end as Rango_Importe
      from   mpolicap b,
             mpolizas a,
             mreporte g,
             tunideco h,
             tramos   i,
             mpolagen1 q2
      where  ---
             q2.cduniage = to_number($P{OTVALOR01})   and
             q2.cdramo   = to_number($P{OTVALOR02})  and
             q2.estado   = 'M'             and
             q2.nmsuplem = (select max(q3.nmsuplem)
                           from   mpolagen1 q3
                           where  q3.cdunieco = q2.cdunieco  and
                                  q3.cdramo   = q2.cdramo    and
                                  q3.estado   = q2.estado    and
                                  q3.nmpoliza = q2.nmpoliza) and 
             ---
             i.cdramo    = b.cdramo                and 
             h.cdunieco  = q2.cduniage             and
             g.cdreporte = $P{wCDREPORTE}              and
             b.status    = 'V'                     and
             b.estado    = a.estado                and
             b.nmsuplem  = a.nmsuplem              and
             b.nmpoliza  = a.nmpoliza              and
             b.cdunieco  = a.cdunieco              and
             b.cdramo    = a.cdramo                and 
             a.cdunieco  = q2.cdunieco             and
             a.cdramo    = q2.cdramo               and
             a.estado    = 'M'                     and
             a.nmsuplem  = (select max(c.nmsuplem)
                            from   mpolizas c
                            where  c.estado   = a.estado   and
                                   c.cdunieco = a.cdunieco and
                                   c.cdramo   = a.cdramo   and
                                   c.nmpoliza = a.nmpoliza)
      group by  
             i.dsramo   ,
             h.dsunieco ,
             a.nmpoliza ,
             g.dsreporte
      order by 
             ptcapita
)
group by
     dsramo     ,
     dsunieco   ,  
     dsreporte  ,
     Rango_Importe
order by
     Rango_Importe]]></queryString>

