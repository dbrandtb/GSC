<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="incisos-endpoint" abstract="true"
		parent="parent-endpoint">
		<property name="templateModule" value="incisos" />
		<property name="xslModule" value="incisos" />
	</bean>

<!-- Bean declarado para obtener incisos totales existentes en BD-->

	<bean id="OBTIENE_INCISOS_DEL_PRODUCTO" parent="incisos-endpoint">
		<property name="templateName" value="obtieneIncisosDelProducto.vm" />
		<property name="xslName" value="obtieneIncisos.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>

	<bean id="OBTIENE_INCISOS" parent="incisos-endpoint">
		<property name="templateName" value="obtieneIncisos.vm" />
		<property name="xslName" value="obtieneIncisos.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>

<!-- Bean declarado para agregar un incisos a la BD *NOTA: retorna un valor number
	 de la bd para el MSG_ID pero en este momento no se esta manipulando por eso
	 transformResult = false y unmarshallResult = false -->	

	<bean id="AGREGAR_INCISO" parent="incisos-endpoint">
		<property name="templateName" value="agregarInciso.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>
	
<!-- Bean declarado para asociar un incisos a la BD *NOTA: retorna un valor number
	 de la bd para el MSG_ID pero en este momento no se esta manipulando por eso
	 transformResult = false y unmarshallResult = false -->	

	<bean id="ASOCIAR_INCISO" parent="incisos-endpoint">
		<property name="templateName" value="asociarInciso.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>	
	
	<bean id="ELIMINAR_INCISO" parent="incisos-endpoint">
		<property name="templateName" value="eliminarInciso.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>	
</beans>
