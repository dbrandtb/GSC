<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="opcmenuusuario-endpoint" abstract="true"
        parent="parent-endpoint">
        <property name="templateModule" value="opcmenuusuario" />
        <property name="xslModule" value="opcmenuusuario" />
    </bean>

    <!-- Bean declarado para obtener la configuracion de Menu Usuario existente en BD-->

    <bean id="OBTIENE_MENU" parent="opcmenuusuario-endpoint">
        <property name="templateName" value="obtieneMenuUsuario.vm" />
        <property name="xslName" value="obtieneMenuUsuario.xsl" />
        <property name="unmarshallClass"
            value="java.util.ArrayList" />
    </bean>
    
    <!-- Bean declarado para obtener la configuracion de Menu Usuario existente en BD-->

    <bean id="OBTIENE_OPCIONES_MENU" parent="opcmenuusuario-endpoint">
        <property name="templateName" value="obtieneOpcionesMenuUsuario.vm" />
        <property name="xslName" value="obtieneOpcionesMenuUsuario.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
    </bean>
    
    <bean id="EXPORT_OPCIONES_MENU" parent="opcmenuusuario-endpoint">
        <property name="templateName" value="obtieneOpcionesMenuUsuario.vm" />
        <property name="xslName" value="obtieneOpcionesMenuUsuarioExport.xsl" />
        <property name="unmarshallClass"
            value="java.util.ArrayList" />
    </bean>
    
    <!-- Bean declarado para obtener roles totales existentes en BD-->

    <bean id="OBTIENE_ROLES" parent="opcmenuusuario-endpoint">
        <property name="templateName" value="obtieneRoles.vm" />
        <property name="xslName" value="obtieneRoles.xsl" />
        <property name="unmarshallClass"
            value="java.util.ArrayList" />
    </bean>
    
    <!-- Bean declarado para obtener lista de clientes-->
    
    <bean id="OBTIENE_CLIENTES" parent="opcmenuusuario-endpoint">
        <property name="templateName" value="obtieneClientes.vm" />
        <property name="xslName" value="obtieneClientes.xsl" />
        <property name="unmarshallClass"
            value="java.util.ArrayList" />
    </bean>
    
    <!-- Bean declarado para obtener los usuarios -->
        
    <bean id="OBTIENE_USUARIOS" parent="opcmenuusuario-endpoint">
        <property name="templateName" value="obtieneUsuarios.vm" />
        <property name="xslName" value="obtieneUsuarios.xsl" />
        <property name="unmarshallClass"
            value="java.util.ArrayList" />
    </bean>
    
    <!-- Bean declarado para obtener los usuarios -->
        
    <bean id="OBTIENE_OPCIONES" parent="opcmenuusuario-endpoint">
        <property name="templateName" value="obtieneOpciones.vm" />
        <property name="xslName" value="obtieneOpciones.xsl" />
        <property name="unmarshallClass"
            value="java.util.ArrayList" />
    </bean>
    
    <!-- Bean declarado para obtener los productos de clientes -->
        
    <bean id="OBTIENE_PRODUCTO_CLIENTES" parent="opcmenuusuario-endpoint">
        <property name="templateName" value="obtieneProductosClientes.vm" />
        <property name="xslName" value="obtieneProductosClientes.xsl" />
        <property name="unmarshallClass"
            value="java.util.ArrayList" />
    </bean>
    
    <!-- Bean declarado para obtener los productos de clientes -->
        
    <bean id="OBTIENE_NIVEL_MENU" parent="opcmenuusuario-endpoint">
        <property name="templateName" value="obtieneNivelMenu.vm" />
        <property name="xslName" value="obtieneNivelMenu.xsl" />
        <property name="unmarshallClass"
            value="java.util.ArrayList" />
    </bean>
    
    <!-- Bean declarado para obtener lista de tipos-->
    
    <bean id="OBTIENE_TIPOS" parent="opcmenuusuario-endpoint">
        <property name="templateName" value="obtieneTipos.vm" />
        <property name="xslName" value="obtieneTipos.xsl" />
        <property name="unmarshallClass"
            value="java.util.ArrayList" />
    </bean>
    
    <!-- Bean declarado para obtener lista de tipos-->
    
    <bean id="OBTIENE_TIPOS_SITUACION" parent="opcmenuusuario-endpoint">
        <property name="templateName" value="obtieneTiposSituacion.vm" />
        <property name="xslName" value="obtieneTiposSituacion.xsl" />
        <property name="unmarshallClass"
            value="java.util.ArrayList" />
    </bean>
    
    <!--Bean declarado para guardar una nueva configuracion-->
    
    <bean id="GUARDA_CONFIG_OPCION_MENU" parent="opcmenuusuario-endpoint">
        <property name="templateName" value="guardaConfigOpcionMenu.vm" />
        <property name="xslName" value="obtieneMensaje.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.model.MensajesVO" />
    </bean>
    
    <!-- Bean decalarado para copiar una configuracion -->
    <bean id="COPIA_CONFIGURACION" parent="opcmenuusuario-endpoint">
        <property name="templateName" value="copiaConfiguracion.vm"/>
        <property name="transformResult" value="false" />         
        <property name="unmarshallResult" value="false" />         
    </bean>                                                          
    <!-- Bean declarado para borrar una configuracion de Menu -->                
    <bean id="BORRA_CONFIG_OPCION_MENU" parent="opcmenuusuario-endpoint">      
        <property name="templateName" value="borraConfigOpcionMenu.vm"/>
        <property name="xslName" value="obtieneMensaje.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.model.MensajesVO" />
    </bean>
    <!--Bean para la consulta de los datos completos de configuraciones para la exportacion  -->
     <bean id="EXPORT_CONFIGURACIONES" parent="opcmenuusuario-endpoint" >
        <property name="templateName" value="obtieneRoles.vm" />
        <property name="xslName" value="exportConfiguracion.xsl" />
        <property name="unmarshallClass" value="java.util.ArrayList" />
    </bean>
</beans>