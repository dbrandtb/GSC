<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="principal-endpoint" abstract="true"
		parent="parent-endpoint">
		<property name="templateModule" value="principal" />
		<property name="xslModule" value="principal" />
	</bean>

	<!-- Bean declarado para obtener roles totales existentes en BD-->

	<bean id="PRINCIPAL_OBTIENE_ROLES" parent="principal-endpoint">
		<property name="templateName" value="obtieneRoles.vm" />
		<property name="xslName" value="obtieneRoles.xsl" />
		<!--property name="unmarshallClass"
			value="java.util.ArrayList" /-->
		<property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
	</bean>
	
	<!-- Bean declarado para obtener lista de clientes-->
	
	<bean id="OBTIENE_CLIENTES" parent="principal-endpoint">
		<property name="templateName" value="obtieneClientes.vm" />
		<property name="xslName" value="obtieneClientes.xsl" />
		<property name="unmarshallClass"
			value="java.util.ArrayList" />
	</bean>
	
	<!-- Bean declarado para obtener lista de Secciones-->
	
	<bean id="OBTIENE_SECCIONES" parent="principal-endpoint">
		<property name="templateName" value="obtieneSecciones.vm" />
		<property name="xslName" value="obtieneSecciones.xsl" />
		<property name="unmarshallClass"
			value="java.util.ArrayList" />
	</bean>
	
	<!-- Bean declarado para obtener lista de Roles-->
	
	<bean id="CARGA_ROLES" parent="principal-endpoint">
		<property name="templateName" value="cargaRoles.vm" />
		<property name="xslName" value="cargaRoles.xsl" />
		<property name="unmarshallClass"
			value="java.util.ArrayList" />
	</bean>
	
	<!-- Bean declarado para obtener lista de tipos-->
	
	<bean id="OBTIENE_TIPOS" parent="principal-endpoint">
		<property name="templateName" value="obtieneTipos.vm" />
		<property name="xslName" value="obtieneTipos.xsl" />
		<property name="unmarshallClass"
			value="java.util.ArrayList" />
	</bean>
	
	<bean id="OBTIENE_TIPOS_PAG" parent="principal-endpoint">
		<property name="templateName" value="obtieneTiposPag.vm" />
		<property name="xslName" value="obtieneTiposPag.xsl" />
		<property name="unmarshallClass"
			value="java.util.ArrayList" />
	</bean>
	
	
	
	<!--Bean declarado para guardar una nueva configuracion-->
	<bean id="GUARDAR_NUEVA_CONFIGURACION" parent="principal-endpoint">
		<property name="templateName" value="guardaNuevaConfiguracion.vm" />
		<property name="xslName" value="guardaNuevaConfiguracion.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
	</bean>

	
	<bean id="GUARDA_CONFIGURACION" parent="principal-endpoint">
		<property name="templateName" value="guardaConfiguracion.vm" />
		<property name="xslName" value="guardaConfiguracion.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
	</bean>
	
	<!-- Bean decalarado para copiar una configuracion -->
	<bean id="COPIA_CONFIGURACION" parent="principal-endpoint">
		<property name="templateName" value="copiaConfiguracion.vm"/>
		<property name="transformResult" value="false" />         
        <property name="unmarshallResult" value="false" />         
	</bean>                                                          
	<!-- Bean declarado para borrar una configuracion -->                
	<bean id="BORRA_CONFIGURACION" parent="principal-endpoint">      
		<property name="templateName" value="borraConfiguracion.vm"/>
		<property name="xslName" value="borraConfiguracion.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
		<!--property name="transformResult" value="false" />         
        <property name="unmarshallResult" value="false" /-->
	</bean>
	<!--Bean para la consulta de los datos completos de configuraciones para la exportacion  -->
	 <bean id="EXPORT_CONFIGURACIONES" parent="principal-endpoint" >
        <property name="templateName" value="exportConfiguraciones.vm" />
        <property name="xslName" value="exportConfiguracion.xsl" />
        <property name="unmarshallClass" value="java.util.ArrayList" />
    </bean>
    <bean id="OBTIENE_PAGINAS" parent="principal-endpoint">
		<property name="templateName" value="obtienePagina.vm" />
		<property name="xslName" value="paginas.xsl" />
		<property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
	</bean>
<!-- 	<bean id="GUARDA_PAGINA" parent="principal-endpoint">
		<property name="templateName" value="guardaPagina.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>
-->

    <bean id="GUARDA_PAGINA" parent="principal-endpoint" >
        <property name="templateName" value="guardaPagina.vm" />
        <property name="xslName" value="guardaPagina.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
    </bean>	
	
	
	
	<bean id="BORRA_PAGINA" parent="principal-endpoint">      
		<property name="templateName" value="borraPagina.vm"/>
		<property name="transformResult" value="false" />         
        <property name="unmarshallResult" value="false" />
	</bean>
	 <bean id="EXPORT_PAGINAS" parent="principal-endpoint" >
        <property name="templateName" value="obtienePagina.vm" />
        <property name="xslName" value="exportPaginas.xsl" />
        <property name="unmarshallClass" value="java.util.ArrayList" />
    </bean>
	
</beans>