<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
    
    <bean id="procesoscriticos-endpoint" abstract="true" parent="parent-endpoint" >
        <property name="templateModule" value="procesosCriticos" />
        <property name="xslModule" value="procesosCriticos" />
    </bean>

    <bean id="CONFIRMA_PASSWORD" parent="procesoscriticos-endpoint">
    	<property name="templateName" value="confirmaPassword.vm"/>
    	<property name="xslName" value="confirmaPassword.xsl"/>
    	<property name="unmarshallClass" value="mx.com.aon.portal.model.MensajesVO"/>
    </bean>
    
</beans>