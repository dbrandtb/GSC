<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
    
    <bean id="test-endpoint" abstract="true" parent="parent-endpoint" >
        <property name="templateModule" value="test" />
        <property name="xslModule" value="test" />
    </bean>

	<!-- 
	Bean agregado para el servicio de consulta de informacion paginada de los recibos
	-->
    <bean id="CONSULTA_PAGINADA" parent="test-endpoint" >
        <property name="templateName" value="consultaPaginada.vm" />
        <property name="xslName" value="consultaPaginada.xsl" />
        <property name="unmarshallClass" value="java.util.ArrayList" />
    </bean>

	<!-- 
	Bean para la consulta de los datos completos de recibos para la exportacion
	-->
    <bean id="EXPORT_RECIBO" parent="test-endpoint" >
        <property name="templateName" value="exportRecibo.vm" />
        <property name="xslName" value="exportRecibo.xsl" />
        <property name="unmarshallClass" value="java.util.ArrayList" />
    </bean>

</beans>
