<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="datosFijos-endpoint" abstract="true"
		parent="parent-endpoint">
		<property name="templateModule" value="datosFijos" />
		<property name="xslModule" value="datosFijos" />
	</bean>

<!-- Bean declarado para obtener los combos de coberturas existentes en BD-->

	<bean id="OBTENER_DATOS_FIJOS_PRODUCTO" parent="datosFijos-endpoint">
		<property name="templateName" value="listaDatosFijos.vm" />
		<property name="xslName" value="listaDatosFijos.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	<bean id="OBTENER_CATALOGO_BLOQUES" parent="datosFijos-endpoint">
		<property name="templateName" value="listaBloque.vm" />
		<property name="xslName" value="listaCombosTresAtributos.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
		<bean id="OBTENER_CATALOGO_CAMPOS" parent="datosFijos-endpoint">
		<property name="templateName" value="listaCampo.vm" />
		<property name="xslName" value="listaCombos.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>
	
	<bean id="INSERTAR_DATO_FIJO" parent="datosFijos-endpoint">
		<property name="templateName" value="insertarDatoFijo.vm" />
		<property name="transformResult" value="false" />
        <property name="unmarshallResult" value="false" />
	</bean>
</beans>
