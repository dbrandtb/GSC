<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
    
    <bean id="configworkflow-endpoint" abstract="true" parent="parent-endpoint">
        <property name="templateModule" value="configworkflow" />
        <property name="xslModule" value="configworkflow" />
    </bean>
    
    <bean id="INSERTA_PROCESOS" parent="configworkflow-endpoint" >
        <property name="templateName" value="insertaProcesos.vm" />
        <property name="xslName" value="obtieneMensaje.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.model.configworkflow.WorkFlowPs1VO" />
    </bean>
    
    <bean id="BORRA_PROCESOS" parent="configworkflow-endpoint" >
        <property name="templateName" value="borraProcesos.vm" />
        <property name="xslName" value="obtieneMensaje.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.model.configworkflow.WorkFlowPs1VO" />
    </bean>
    
    <bean id="OBTIENE_PROCESOSXCTA_REG" parent="configworkflow-endpoint" >
        <property name="templateName" value="obtieneProcesosXCtaReg.vm" />
        <property name="xslName" value="obtieneProcesosXCtaReg.xsl" />
        <property name="unmarshallClass" value="java.util.ArrayList" />
    </bean>
    
    <bean id="OBTIENE_PROCESOSXCTA" parent="catalogs-endpoint" >
        <property name="templateName" value="obtieneProcesosXCta.vm" />
        <property name="xslName" value="obtieneProcesosXCta.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
    </bean>
    
    <bean id="OBTIENE_PASOSXCTA" parent="configworkflow-endpoint" >
        <property name="templateName" value="obtienePasosXCta.vm" />
        <property name="xslName" value="obtienePasosXCta.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
    </bean>
    
    <bean id="INSERTA_PASOSXCTA" parent="configworkflow-endpoint" >
        <property name="templateName" value="insertaPasos.vm" />
        <property name="xslName" value="obtieneMensajePasos.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.model.configworkflow.WorkFlowPs2VO" />
    </bean>
    
    <bean id="BORRA_PASOSXCTA" parent="configworkflow-endpoint" >
        <property name="templateName" value="borraPasos.vm" />
        <property name="xslName" value="obtieneMensajePasos.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.model.configworkflow.WorkFlowPs2VO" />
    </bean>

    <!-- Bean declarado para exportar a diferentes formatos los planes existentes en la BD -->    
    <bean id="WF_EXPORT" parent="configworkflow-endpoint">
        <property name="templateName" value="obtienePasosXCta.vm" />
        <property name="xslName" value="exportWorkflow.xsl" />
        <property name="unmarshallClass" value="java.util.ArrayList" />
    </bean>
</beans>