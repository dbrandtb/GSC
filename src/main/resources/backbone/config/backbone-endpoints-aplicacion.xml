<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="aplicacion-endpoint" abstract="true"
		parent="parent-endpoint">
		<property name="templateModule" value="aplicacion" />
		<property name="xslModule" value="aplicacion" />
	</bean>

	<bean id="OBTIENE_TABLA_APOYO" parent="aplicacion-endpoint">
		<property name="templateName" value="listaTablasApoyo.vm" />
		<property name="xslName" value="listaCombosTabApoyo.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>	 
    
	<bean id="OBTIENE_TIPO_CONCEPTOS" parent="aplicacion-endpoint">
		<property name="templateName" value="listaTipoConceptos.vm" />
		<property name="xslName" value="listaCombosTipo.xsl" />
		<property name="unmarshallClass" value="java.util.ArrayList" />
	</bean>	
    
</beans>