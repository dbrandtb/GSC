<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="renovacion-endpoint" abstract="true"
        parent="parent-endpoint">
        <property name="templateModule" value="flujos/renovacion" />
        <property name="xslModule" value="flujos/renovacion" />
    </bean>

    <bean id="RENOVACION_OBTIENE_POLIZAS" parent="renovacion-endpoint">
        <property name="templateName" value="renovacionObtienePolizas.vm" />
        <property name="xslName" value="renovacionObtienePolizas.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
    </bean>

    <bean id="RENOVACION_OBTIENE_POLIZAS_RENOVADAS" parent="renovacion-endpoint">
        <property name="templateName" value="renovacionObtienePolizasRenovadas.vm" />
        <property name="xslName" value="renovacionObtienePolizasRenovadas.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
    </bean>

    <bean id="RENOVACION_GUARDA_POLIZA_SELECCION" parent="renovacion-endpoint">
        <property name="templateName" value="renovacionGuardaPolizaSeleccion.vm" />
        <property name="xslName" value="renovacionGuardaPolizas.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
    </bean>

	<bean id="RENOVACION_BORRADOR_POLIZAS" parent="renovacion-endpoint">
        <property name="templateName" value="renovacionBorradorPolizas.vm" />
        <property name="xslName" value="renovacionBorradorPolizas.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
    </bean>
    <bean id="RENOVACION_RENOVAR_POLIZAS" parent="renovacion-endpoint">
        <property name="templateName" value="renovacionRenovarPolizas.vm" />
        <property name="xslName" value="renovacionRenovarPolizas.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
    </bean>
    <bean id="RENOVACION_OBTIENE_MENSAJE" parent="renovacion-endpoint">
        <property name="templateName" value="renovacionObtieneMensaje.vm" />
        <property name="xslName" value="renovacionObtieneMensaje.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
    </bean>
    
    <bean id="RENOVACION_APROBAR_POLIZAS" parent="renovacion-endpoint">
        <property name="templateName" value="renovacionAprobarPolizas.vm" />
        <property name="xslName" value="renovacionAprobarPolizas.xsl" />
        <property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" />
    </bean>
    
    <bean id="RENOVACION_OBTIENE_POLIZAS_RENOVADAS_EXPORTAR" parent="renovacion-endpoint">
        <property name="templateName" value="renovacionObtienePolizasRenovadas.vm" />
        <property name="xslName" value="renovacionObtienePolizasRenovadasExportar.xsl" />
        <property name="unmarshallClass" value="java.util.ArrayList" />
    </bean>
    
    <bean id="RENOVACION_OBTIENE_POLIZAS_EXPORTAR" parent="renovacion-endpoint">
        <property name="templateName" value="renovacionObtienePolizas.vm" />
        <property name="xslName" value="renovacionObtienePolizasExportar.xsl" />
        <property name="unmarshallClass" value="java.util.ArrayList" />
    </bean>

	<bean id="OBTIENE_POLIZA_RENOVAR_EXPORT" parent="renovacion-endpoint">
  		<property name="templateName" value="renovacionObtienePolizasExport.vm" /> 
  		<property name="xslName" value="renovacionObtienePolizasExport.xsl" /> 
  		<property name="unmarshallClass" value="mx.com.aon.portal.util.WrapperResultados" /> 
	</bean>
	
	<bean id="VALIDA_ENDOSO_POLIZA_RENOVA" parent="renovacion-endpoint">
        <property name="templateName" value="validaEndosoPolizaRenova.vm" />
        <property name="xslModule" value="emision" />
        <property name="xslName" value="validaEndosoPoliza.xsl" />
		<property name="unmarshallClass" value="mx.com.aon.portal.model.BaseObjectVO" />
    </bean>

</beans>