<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>


	<include file="struts-default.xml" />

	<package name="statusCasos" extends="json-default" namespace="/statusCasos">


    <action name="buscarStatusCasos" class="listaStatusCasosAction" method="cmdBuscarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
	</action>
    
    <action name="borrarStatusCasos" class="statusCasosAction" method="cmdBorrarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>

    <action name="obtenerStatusCasos" class="statusCasosAction" method="cmdGetClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
    
    <action name="guardarStatusCasos" class="statusCasosAction" method="cmdGuardarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>

    <action name="buscarStatusCasosTareas" class="listaStatusCasosAction" method="cmdBuscarStatusCasosTareasClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>

    <action name="guardarStatusCasosTareas" class="statusCasosAction" method="cmdGuardarStatusCasosTareaClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
    
    <action name="borrarStatusCasosTareas" class="statusCasosAction" method="cmdBorrarStatusCasosTareaClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
    
    <action name="exportarStatusCasos" class="listaStatusCasosAction" method="cmdExportarSttCsClick">
        <result name="success" type="stream">
             <param name="contentType">${contentType}</param>
            <param name="inputName">inputStream</param>
            <param name="contentDisposition">filename="${filename}"</param>
            <param name="bufferSize">4096</param>
        </result>
    </action>  
    
    <action name="buscarStatusCasosTareasProcesos" class="listaStatusCasosAction" method="cmdBuscarStatusCasosTareasProcesosClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>    
    
    
    
    

	</package>

</struts>
