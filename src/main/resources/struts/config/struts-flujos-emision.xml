<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <include file="struts-default.xml" />

    
    <package name="procesoemision" extends="json-default" namespace="/procesoemision">
    
        <interceptors>
            <interceptor-stack name="defaultStack">
                <interceptor-ref name="exception" />
                <interceptor-ref name="alias" />
                <interceptor-ref name="servletConfig" />
                <interceptor-ref name="prepare" />
                <interceptor-ref name="i18n" />
                <interceptor-ref name="chain" />
                <interceptor-ref name="modelDriven" />
                <interceptor-ref name="fileUpload" />
                <interceptor-ref name="staticParams" />
                <interceptor-ref name="params" />
                <interceptor-ref name="conversionError" />
                <interceptor-ref name="validation">
                    <param name="excludeMethods">cancel,execute,delete,edit,list</param>
                </interceptor-ref>
                <interceptor-ref name="workflow">
                    <param name="excludeMethods">input,back,cancel</param>
                </interceptor-ref>
            </interceptor-stack>
        </interceptors>
        
        <!-- Hace el redireccionamiento al JSP -->
        <action name="busquedaPoliza" class="emisionAction" method="busquedaPoliza">
            <result name="input">/jsp/procesos/emision/busquedaPoliza.jsp</result>        
        </action>
       
        <!-- Busca las Polizas existentes -->
        <action name="buscaPolizas" class="emisionAction" method="buscaPolizas">
            <result name="success" type="json" />
        </action>
            
        <!-- Obtiene los Productos para llenar el combo -->
        <action name="obtenerProductos" class="emisionAction"  method="obtenerProductos" >
            <result name="success" type="json" />
        </action>
        
        <!-- Obtiene las Aseguradoras para llenar el combo -->
        <action name="obtenerAseguradoras" class="emisionAction"  method="obtenerAseguradoras" >
            <result name="success" type="json" />
        </action>
        
        <!-- Obtiene los status de Poliza para llenar el combo -->
        <action name="obtenerStatusPoliza" class="emisionAction"  method="obtenerStatusPoliza" >
            <result name="success" type="json" />
        </action>
        
        <!-- Hace el redireccionamiento al JSP de Detalle de la Poliza -->
        <action name="detallePoliza" class="emisionAction" method="obtienePantalla">
            <result name="input">/jsp/procesos/emision/detallePoliza.jsp</result>        
        </action>
        
        <!-- Obtiene el detalle de la Poliza -->
        <action name="consultaDetallePoliza" class="emisionAction"  method="consultaDetallePoliza" >
            <result name="success" type="json" />
        </action>
        
        <!-- Consulta Objeto Asegurable de Poliza -->
        <action name="consultaObjetoAsegurable" class="emisionAction"  method="consultaObjetoAsegurable" >
            <result name="success" type="json" />
        </action>
        
        <!-- Consulta Funcion de Poliza -->
        <action name="consultaFuncionPoliza" class="emisionAction"  method="consultaFuncionPoliza" >
            <result name="success" type="json" />
        </action>
        
        <!-- ValidaciÃ³n Poliza Endosos -->
        <action name="validacionPolizaEndosos" class="emisionAction"  method="validacionPolizaEndosos" >
            <result name="success" type="json" />
        </action>
        
        <!-- Carga informacion de recibos de la poliza -->
        <action name="recibos" class="emisionAction" method="consultaDetalleRecibos">
            <result name="success" type="json" />        
        </action>
        
        <!-- Carga informacion del detalle de recibos de la poliza -->
        <action name="recibosDetalle" class="emisionAction" method="consultaRecibosDetalle">
            <result name="success" type="json" />        
        </action>
        
             
        <!-- Carga informacion del detalle de recibos de la poliza provenientes de polizas canceladas-->
        <action name="polizasCanceladasRecibosDetalle" class="emisionAction" method="consultaPolizasCanceladasRecibosDetalle">
            <result name="success" type="json" />        
        </action>
        
        
        <!-- Consulta Datos Adicionales de Poliza -->
        <action name="consultaDatosAdicionalesPoliza" class="emisionAction"  method="obtieneDatosAdicionales" >
            <result name="success" type="json" />
        </action>
        
        <!-- Consulta de Datos Rol -->
        <action name="datosRol" class="emisionAction" method="obtenerPantallaDatosRol">
            <result name="success" type="json" />
        </action>
       
       <!-- Consulta de Coberturas -->
       <action name="datosCoberturas" class="emisionAction" method="obtenerPantallaCoberturas">
            <result name="success" type="json" />
            <!-- <result name="success">/jsp/procesos/emision/datosCoberturas.jsp</result> --> 
       </action>
       
       <!-- Consulta de Accesorios -->
       <action name="datosAccesorios" class="emisionAction" method="obtenerPantallaAccesorios">
            <result name="success" type="json" />
            <!-- <result name="success">/jsp/procesos/emision/datosAccesorios.jsp</result> -->
       </action>
       
        <!-- Consulta Agrupador -->
        <action name="consultaAgrupador" class="emisionAction"  method="obtenerAgrupador" >
            <result name="success" type="json" />
        </action>
       
       <!-- Pantalla Incisos -->
       <action name="datosIncisos" class="emisionAction" method="obtenerPantallaIncisos">
            <result name="success">/jsp/procesos/emision/datosIncisos.jsp</result>
       </action>
       
       <!-- Consulta de Coberturas Grid Incisos -->
       <action name="coberturasIncisos" class="emisionAction" method="coberturasGridJson">
            <result name="success" type="json" />
       </action>
       
       <!-- Consulta de Accesorios Grid Incisos -->
       <action name="accesoriosIncisos" class="emisionAction" method="accesoriosGridJson">
            <result name="success" type="json" />
       </action>
       
       <!-- Consulta de Accesorios Grid Incisos -->
       <action name="ayudaCobertura" class="emisionAction" method="obtenerAyudaCobertura">
            <result name="success" type="json" />
       </action>
       
       <!-- Consulta Polizas Maestras -->
       <action name="consultaPolizasMaestras" class="emisionAction">
            <result name="input">/jsp/flujos/emision/consultaPolizaMaestra.jsp</result>
       </action>

       <action name="getComboNivel" class="emisionAction" method="obtenerComboNivel">
            <result name="success" type="json" />
       </action>
       <action name="getComboAseguradora" class="emisionAction" method="obtenerComboAseguradora">
            <result name="success" type="json" />
       </action>
       
       <!-- Consulta aseguradoras por cliente -->                               
       <action name="getComboAseguradoraPorCliente" class="emisionAction" method="obtenerComboAseguradoraPorCliente">
            <result name="success" type="json" />
       </action>
       
       <action name="getComboProducto" class="emisionAction" method="obtenerComboProducto">
            <result name="success" type="json" />
       </action>
       
       <action name="getComboProductoPorAseguradora" class="emisionAction" method="obtenerComboProductoPorAseguradora">
            <result name="success" type="json" />
       </action>
       
       <action name="getComboTipos" class="emisionAction" method="obtenerComboTipos">
            <result name="success" type="json" />
       </action>
       
       <action name="getComboTipoDeSituacion" class="emisionAction" method="obtenerComboTipoDeSituacion">
            <result name="success" type="json" />
       </action>
       
       <action name="guardaPolizaMaestra" class="emisionAction" method="guardaPolizaMaestra">
            <result name="success" type="json" />
       </action>
       <action name="obtienePolizaMaestra" class="emisionAction" method="obtienePolizaMaestra">
            <result name="success" type="json"></result>
       </action>
       
       <action name="irAgrupacionPorRol" class="emisionAction"  method="irAgrupacionPorRolClick">
	        <result type="chain" name="agrupacionPapel">
	          <param name="actionName">agrupacionPapel</param>
	          <param name="namespace">/agrupacionPapel</param>
	        </result>
    	</action>

       <action name="irAgrupacionPorGrupoPersona" class="emisionAction"  method="irAgrupacionPorGrupoPersonaClick">
	        <result type="chain" name="agrupacionGrupoPersonas">
	          <param name="actionName">agrupacionGrupoPersonas</param>
	          <param name="namespace">/agrupacionGrupoPersonas</param>
	        </result>
    	</action>

    	
     <!-- Llamda a la pantalla numeracion de poliza -->
       <action name="irAsignarNumero" class="emisionAction" method="cmdIrAsignarNumero">
            <result type="chain" name="numeroPolizas">numeroPolizas</result>
        </action>
    	
       <action name="irCancelacionManual" class="emisionAction"  method="irCancelacionManualClick">
	        <result type="chain" name="cancelacionManualPolizas">
	          <param name="actionName">cancelacionManualPolizas</param>
	          <param name="namespace">/cancelacionManualPolizas</param>
	        </result>
    	</action>
    	
    	<action name="irPolizasCanceladas" class="emisionAction"  method="irPolizasCanceladasClick">
	         <result type="chain" name="polizasCanceladas">
	            <param name="actionName">polizasCanceladas</param>
	          <param name="namespace">/Polizas</param> 
	        </result>
    	</action>
    	<action name="irPolizasARenovar" class="emisionAction"  method="irPolizasARenovarClick">
	         <result type="chain" name="renovacionPolizas">
	            <param name="actionName">renovacionPolizas</param>
	          <param name="namespace">/flujorenovacion</param> 
	        </result>
    	</action>
    	
		<action name="irPolizasRenovadas" class="endosoAction"  method="irPolizasRenovadasClick">
	         <result type="chain" name="polizasRenovadas">
	            <param name="actionName">polizasRenovadas</param>
	          <param name="namespace">/flujorenovacion</param> 
	        </result>
    	</action>
    	<action name="irRehabilitacionMasiva" class="emisionAction"  method="irRehabilitacionMasivaClick">
	         <result type="chain" name="rehabilitacionMasiva">
	            <param name="actionName">rehabilitacionMasiva</param>
	          <param name="namespace">/</param> 
	        </result>
    	</action>
       
    </package>
</struts>