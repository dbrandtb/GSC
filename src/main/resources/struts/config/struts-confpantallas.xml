<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

	<package name="confpantallas" namespace="/confpantallas"
		extends="default">

		<action name="grabapanel" class="mx.com.gseguros.confpantallas.action.GrabaPanel">
			<result name="SUCCESS">/jsp/confpantallas/success.jsp</result>
		</action>
		<action name="cargainfo" class="mx.com.gseguros.confpantallas.action.CargaInfo">
			<result name="SUCCESS">/jsp/confpantallas/comboresp.jsp</result>
		</action>
		<action name="pintapanel" class="mx.com.gseguros.confpantallas.action.PintaPanel">
			<result name="SUCCESS">/jsp/confpantallas/commonJson.jsp</result>
		</action>
		<action name="pintatreepanel"
			class="mx.com.gseguros.confpantallas.action.PintaTreePanel">
			<result name="success">/jsp/confpantallas/treepanel.jsp</result>
		</action>
		<action name="buscacontrol"
			class="mx.com.gseguros.confpantallas.action.BuscaControl">
			<result name="SUCCESS">/jsp/confpantallas/data.jsp</result>
		</action>

		<action name="pintajson" class="mx.com.gseguros.confpantallas.action.PintaJson">
			<result name="SUCCESS">/jsp/confpantallas/commonJson.jsp</result>
		</action>
		<action name="cotizacionDemo" class="CotizacionAction" method="pantallaCotizacionDemo">
			<result name="success">/jsp-script/proceso/emision/cotizacionDemo.jsp
			</result>
		</action>

	</package>

</struts>