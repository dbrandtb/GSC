<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>


	<include file="struts-default.xml" />

	<package name="administracionAtributosFax" extends="json-default" namespace="/administracionAtributosFax">


    <action name="buscarAdministracionAtributosFax" class="listaAdministracionAtributosFaxAction" method="cmdBuscarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
	</action>
    
    <action name="borrarAdministracionAtributosFax" class="administracionAtributosFaxAction" method="cmdBorrarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>

    <action name="getAdministracionAtributosFax" class="administracionAtributosFaxAction" method="cmdGetClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>

    <action name="guardarAdministracionAtributosFax" class="administracionAtributosFaxAction" method="cmdGuardarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
    </action>
    
    <action name="exportarAdministracionAtributosFax" class="listaAdministracionAtributosFaxAction" method="cmdExportarAdmAtrFaxClick">
        <result name="success" type="stream">
             <param name="contentType">${contentType}</param>
            <param name="inputName">inputStream</param>
            <param name="contentDisposition">filename="${filename}"</param>
            <param name="bufferSize">4096</param>
        </result>
    </action>  
    

	</package>

</struts>
