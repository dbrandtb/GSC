<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

    <include file="struts-default.xml" />                           

    <package name="ayudaCoberturas" extends="json-default" namespace="/ayudaCoberturas">
    
    	<action name="getAyudaCoberturas" class="ayudaCoberturasAction" method="cmdGetClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
        </action>
		
		<action name="buscarAyudaCoberturas" class="listaAyudaCoberturasAction" method="cmdBuscarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
		</action>
		
		
		 <action name="exportarAyudaCoberturas" class="listaAyudaCoberturasAction" method="cmdExportarClick">
			<result name="success" type="stream">
				<param name="contentType">${contentType}</param>
				<param name="inputName">inputStream</param>
				<param name="contentDisposition">filename="${filename}"</param>
				<param name="bufferSize">4096</param>
			</result>
		</action>        
			
	   <action name="insertarAyudaCoberturas" class="ayudaCoberturasAction" method="cmdInsertarClick">
           <result type="json"><param name="ignoreHierarchy">false</param></result>
       </action>

       <action name="guardarAyudaCoberturas" class="ayudaCoberturasAction" method="cmdGuardarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
        </action>

        <action name="borrarAyudaCoberturas" class="ayudaCoberturasAction" method="cmdBorrarClick" >
            <result type="json"><param name="ignoreHierarchy">false</param></result>
        </action>

         <action name="copiarAyudaCoberturas" class="ayudaCoberturasAction" method="cmdCopiarClick">
            <result type="json"><param name="ignoreHierarchy">false</param></result>
        </action>
     
    </package>

</struts>