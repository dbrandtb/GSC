	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="principalEmision" class="mx.com.aon.procesos.emision.web.PrincipalEmisionAction" abstract="false"/>
    
	<bean id="emisionAction" class="mx.com.aon.procesos.emision.web.EmisionAction" scope="prototype" parent="principalEmision">
