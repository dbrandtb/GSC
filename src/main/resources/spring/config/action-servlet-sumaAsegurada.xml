<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">


     
  <!-- Tipo de objetos -->    
    <bean id="SumaAseguradaAction" class="mx.com.aon.catweb.configuracion.producto.sumaAsegurada.web.SumaAseguradaAction" scope="prototype" parent="PadreAction">
	    <property name="sumaAseguradaManager" ref="SumaAseguradaManager" />
	    <property name="sumaAseguradaManagerJdbcTemplateImpl" ref="sumaAseguradaManagerJdbcTemplateImpl" />
    </bean>
    
    <!-- sumaAseguradaManagerJdbcTemplateImpl -->
	<bean id="sumaAseguradaManagerJdbcTemplateImpl" class="mx.com.aon.catweb.configuracion.producto.service.impl.SumaAseguradaManagerJdbcTemplateImpl" parent="abstractManagerJdbcTemplateInvoke">
    	<property name="abstractDAO">
        	<ref bean="sumaAseguradaDAO"/>
		</property>
	</bean>
    
</beans>