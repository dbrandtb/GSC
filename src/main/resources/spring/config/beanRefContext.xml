<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<!-- load a hierarchy of contexts, although there is just one here -->
<beans>
    <bean id="serviceBO-core-context" class="org.springframework.context.support.ClassPathXmlApplicationContext">
	<constructor-arg>
	<list>
        <value>/spring/config/daoContext.xml</value>
        <!--value>/spring/config/applicationContext-catbo.xml</value-->
        <value>/spring/config/action-servlet-jasper.xml</value>
        <value>/backbone/config/backbone-jdbc.xml</value>
        <value>/backbone/config/backbone-endpoints.xml</value>
        <!--value>/backbone/config/backbone-endpoints-catbo.xml</value-->
    </list>
	</constructor-arg>
	</bean>
</beans>
