<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
	
	<!-- PrincipalRenovacion -->
    <bean id="principalRenovacion" class="mx.com.aon.flujos.renovacion.web.PrincipalRenovacionAction" abstract="true">
    </bean>
  
	<!-- RenovacionAction -->
	<bean id="renovacionAction" class="mx.com.aon.flujos.renovacion.web.RenovacionAction" scope="prototype" parent="principalRenovacion">
        <property name="renovacionManager" ref="renovacionManager" />
        <property name="exportMediator" ref="exportMediator"/>
        <property name="catalogManager" ref="catalogManager"/>
	</bean>
    
</beans>