<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="consultasPolizaManager" class="mx.com.gseguros.portal.consultas.service.impl.ConsultasPolizaManagerImpl" parent="abstractManagerJdbcTemplateInvoke">
        <property name="abstractDAO">
            <ref bean="consultasPolizaDAO"/>
        </property>
	</bean>
	
	
	<bean id="consultasSiniestroManagerImpl" class="mx.com.gseguros.portal.consultas.service.impl.ConsultasSiniestroManagerImpl">
       <property name="consultasSiniestroDAO" ref="consultasSiniestroDAOImpl"/>
    </bean>
	
	<bean id="consultasManagerImpl" class="mx.com.gseguros.portal.consultas.service.impl.ConsultasManagerImpl">
        <property name="consultasDAO" ref="consultasDAOImpl" />
    </bean> 
	
</beans>